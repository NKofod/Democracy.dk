INSERT INTO ODA.dbo.Sag (id, typeid, kategoriid, statusid, titel, titelkort, offentlighedskode, nummer, nummerprefix, nummernumerisk, nummerpostfix, resume, afstemningskonklusion, periodeid, afgørelsesresultatkode, baggrundsmateriale, opdateringsdato, statsbudgetsag, begrundelse, paragrafnummer, paragraf, afgørelsesdato, afgørelse, rådsmødedato, lovnummer, lovnummerdato, retsinformationsurl, fremsatundersagid, deltundersagid) VALUES (26823, 7, null, 54, N'Meddelelse fra Kommissionen om Finansielt til syn i Europa', N'Finansielt til syn i Europa', N'O', N'KOM (2009) 0252', N'KOM', N'252', N'', N'<a href="http://ec.europa.eu/internal_market/consultations/2009/fin_supervision_may_en.htm" onclick="this.target=''_blank''">Link</a> til Kommissionens side om høring af meddelelsen&nbsp;(høringsfristen er 15. juli 2009) <br />
<br />
Tillæg: <a href="http://register.consilium.europa.eu/pdf/en/09/st10/st10511-ad01.en09.pdf" onclick="this.target=''_blank''">SEK (2009) 715</a> og <a href="http://register.consilium.europa.eu/pdf/da/09/st10/st10511-ad02.da09.pdf" onclick="this.target=''_blank''">SEK (2009) 716</a><br />
<br />
Kommissionen konstaterer i meddelelsen, at den finansielle krise viste, at der var store mangler med hensyn til samarbejde, koordinering, ensartethed og tillid mellem de nationale tilsyn. <br />
<br />
I november 2008 fik en højniveaugruppe med Jacques de Larosi&egrave;re som formand mandat til at foreslå anbefalinger til Kommissionen, om hvordan de europæiske tilsynsordninger kan forbedres.<br />
<br />
Den endelige rapport fra de Larosi&egrave;re-gruppen indeholder et udkast til et nyt finansielt tilsynssystem i Europa. Det centrale element heri er forslagene om at styrke samarbejdet og koordineringen mellem de nationale tilsyn, herunder gennem oprettelse af nye europæiske tilsynsmyndigheder og et europæisk organ, som skal have til opgave at overvåge de risici, som det finansielle system som helhed frembyder.<br />
<br />
Kommissionen støtter anbefalingerne i de Larosi&egrave;re-rapporten om en reform af reguleringen og overvågningen af de finansielle markeder.<br />
