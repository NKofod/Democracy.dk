INSERT INTO ODA.dbo.Sag (id, typeid, kategoriid, statusid, titel, titelkort, offentlighedskode, nummer, nummerprefix, nummernumerisk, nummerpostfix, resume, afstemningskonklusion, periodeid, afgørelsesresultatkode, baggrundsmateriale, opdateringsdato, statsbudgetsag, begrundelse, paragrafnummer, paragraf, afgørelsesdato, afgørelse, rådsmødedato, lovnummer, lovnummerdato, retsinformationsurl, fremsatundersagid, deltundersagid) VALUES (96811, 7, null, 54, N'Forslag til RÅDETS AFGØRELSE om de holdninger, der skal indtages på Den Europæiske Unions vegne på det tredje møde mellem parterne i protokollen om eliminering af ulovlig handel med tobaksvarer', N'Forslag til AFGØRELSE om EU''s holdninger på det tredje møde til protokollen om ulovlig handel med tobaksvarer', N'O', N'KOM (2023) 0722', N'KOM', N'722', N'', N'', null, 159, null, null, N'2023-11-22 15:36:56.490', 0, null, null, null, null, null, null, null, null, null, null, null);
INSERT INTO ODA.dbo.Sag (id, typeid, kategoriid, statusid, titel, titelkort, offentlighedskode, nummer, nummerprefix, nummernumerisk, nummerpostfix, resume, afstemningskonklusion, periodeid, afgørelsesresultatkode, baggrundsmateriale, opdateringsdato, statsbudgetsag, begrundelse, paragrafnummer, paragraf, afgørelsesdato, afgørelse, rådsmødedato, lovnummer, lovnummerdato, retsinformationsurl, fremsatundersagid, deltundersagid) VALUES (96812, 8, null, 21, N'Aktstykke om udmøntning af midlerne til Klimaerhvervsskoler i 2023', N'Aktstykke om udmøntning af midlerne til Klimaerhvervsskoler i 2023', N'O', N'Aktstk. 48', N'Aktstk.', N'48', N'', N'', null, 160, N'TF', null, N'2023-11-30 14:48:41.147', 0, null, 20, N'Børne- og Undervisningsministeriet', N'2023-11-30 00:00:00.000', N'Tiltrådt af et flertal i Finansudvalget den 30-11-2023 (udvalget med undtagelse af Liberal Alliances medlem, der stemte imod aktstykket)
Påtegninger:
Enhedslistens medlem af udvalget bemærker, at EL kan støtte aktstykket, men at EL går ud fra og kraftigt vil opfordre til, at de midler, der er afsat til at styrke erhvervsuddannelserne i de kommende år, udmøntes bredere til gavn for den grønne omstilling, så de også går til de uddannelser, der ikke får andel i klimaerhvervsskolemidlerne via nærværende aktstykke, herunder til uddannelser indenfor fødevarer og jordbrug og uddannelser på det merkantile område, som også skal understøttes yderligere i at bidrage til den grønne omstilling.

Alternativets medlem af udvalget bemærker, at det er positivt, at reserven til Klimaerhvervsskoler udmøntes bredt frem for – som oprindelig planlagt – til tre institutioner. Alternativet bemærker i øvrigt, at investeringsrammen for erhvervsuddannelserne er sat op på forslag til finansloven for 2024 sammenlignet med tidligere finanslove, hvilket er nødvendigt, når intentionen er, at særtilskuddet skal anvendes til investeringer i udstyr, som understøtter den grønne omstilling. Ikke desto mindre udgør investeringsrammerne generelt en begrænsning for investeringer i bæredygtig skoledrift.
