INSERT INTO ODA.dbo.Dokument (id, typeid, kategoriid, statusid, offentlighedskode, titel, dato, modtagelsesdato, frigivelsesdato, paragraf, paragrafnummer, spørgsmålsordlyd, spørgsmålstitel, spørgsmålsid, procedurenummer, grundnotatstatus, dagsordenudgavenummer, opdateringsdato) VALUES (494692, 17, 22, 3, N'O', N'REU alm. del - svar på spm. 449, fra justitsministeren', N'2007-10-01 00:00:00.000', N'2007-10-01 00:00:00.000', N'2007-10-01 16:28:00.540', N'', N'', N'Ministeren bedes redegøre for, hvordan den nye arvelov (L100, vedtaget af Folketinget 1. juni 2007) arvemæssigt stiller ægtefæller, som gennem længere tid ikke har levet sammen, såfremt den ene ægtefælle - uden at de to er blevet skilt - gennem længere tid med den anden ægtefælles viden har levet i forhold med en anden.
 
Begrundelse.
Der tænkes på eksemplet, hvor Æ1 og Æ2 efter mange års ægteskab beslutter ikke længere at ville være sammen, men af hensyn til eksempelvis børnenes fællesarv beslutter ikke at lade sig skille, men alene flytter fra hinanden. Efter en periode kan begge ægtefæller finde en ny livsledsager, hvor da spørgsmålet om Æ1''s og Æ2''s indbyrdes og dermed børnenes arveforhold rejser sig. Der henvises i denne sammenhæng til de i den nye arvelovs særlige bestemmelser om samliv, der må sidestilles med ægteskab, og disse reglers eventuelle betydning for dem, som Æ1 og Æ2 måtte beslutte sig for at indlede samliv med på trods af, at de fortsat er gift.
', N'Spm. om den nye arvelov, til justitsministeren', 494691, N'', N'', null, N'2015-12-05 16:16:25.443');
