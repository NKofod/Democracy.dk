INSERT INTO ODA.dbo.Dokument (id, typeid, kategoriid, statusid, offentlighedskode, titel, dato, modtagelsesdato, frigivelsesdato, paragraf, paragrafnummer, spørgsmålsordlyd, spørgsmålstitel, spørgsmålsid, procedurenummer, grundnotatstatus, dagsordenudgavenummer, opdateringsdato) VALUES (560765, 17, 22, 1, N'O', N'UDU alm. del - svar på spm. 319, fra undervisningsministeren', N'2009-06-22 00:00:00.000', N'2009-06-22 00:00:00.000', N'2009-06-23 11:19:25.000', N'', N'', N'Finder ministeren, at det strider mod folkeskolelovens § 25, stk. 2, når Frederiksberg Kommune og bestyrelsen ved Skolen på Nyelandsvej på Frederiksberg har besluttet at sammenlægge fire skoleklasser til tre, når det fremgår af folkeskoleloven, at man skal tilstræbe at undgå at bryde op på klassemønstre i indskolingen, og hvad er i øvrigt ministerens mening om sagen?

Spørgeren henviser til omtale i Frederiksberg Bladet 26/5-09 (For fire måneder siden var vi glade), hvoraf det fremgår, at flere forældre på Skolen på Nyelandsvej på Frederiksberg er utilfredse med, at der er truffet beslutninger om at sammenlægge fire børnehaveklasser til tre og dermed bryde med folkeskolelovens hensigtserklæring om at undgå at bryde klassemønstre i indskolingen. Samtidig har forældrene hverken kunnet få formanden for Børne- og Undervisningsudvalget i kommunen eller skolen i tale om sagen. Spørgeren mener, at der er tale om et yderst uheldigt forløb, hvor folkeskoleloven tilsyneladende er blevet tilsidesat og ønsker ministerens vurdering heraf.
