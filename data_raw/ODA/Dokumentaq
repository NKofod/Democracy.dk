INSERT INTO ODA.dbo.Dokument (id, typeid, kategoriid, statusid, offentlighedskode, titel, dato, modtagelsesdato, frigivelsesdato, paragraf, paragrafnummer, spørgsmålsordlyd, spørgsmålstitel, spørgsmålsid, procedurenummer, grundnotatstatus, dagsordenudgavenummer, opdateringsdato) VALUES (16184, 17, 22, 1, N'O', N'TRU alm. del - svar på spm. 8, fra transportministeren', N'2012-10-26 00:00:00.000', N'2012-10-26 00:00:00.000', N'2012-10-26 16:16:12.000', N'', N'', N'I besvarelsen af TRU alm. del – spm. 995 hedder det bl.a., at ”lufthavnens samarbejde med konkrete virksomheder er således en sag for lufthavnen selv” og at ”Ifølge de oplysninger som er forelagt Trafikstyrelsen, vedrører projektet i Odense Lufthavn bl.a. en test- og valideringsfacilitet for civile ubemandede luftfartøjer.” 

Der gøres opmærksom på, at et medlem af lufthavnens bestyrelse er direktør for Falck Schmidt Defence Industries, der samarbejder med Boeing flyfabrikker (kendt for udviklingen af droner til militære formål), der ønsker at bruge Odense Lufthavn til testflyvninger af droner. I det pressemateriale, som Odense Lufthavn udsendte d. 17. november 2011, hedder det bl.a., at "udover samarbejdet med Odense Lufthavn har Boeing allerede relationer til de odenseanske virksomheder IFAD og Falck Schmidt Defence Industries”. Disse relationer er blandt andet et aftalenotat mellem Boeing og IFAD om at udforske mulighederne for at støtte Boeings F/A-18E/F Super Hornets krav til luftangrebstræningssystemer, og en aftale mellem Boeing og Falck Schmidt Defence Systems om potentielt brug dels af Falck Schmidt-brændselsceller i genbrugsenergidrevne systemer for militærplatforme dels af komplekse Falck Schmidt-komponenter og materialer i Boeings rotorflyprogrammer.

Ministeren bedes på baggrund af ovenstående oplyse, om han finder det hensigtsmæssigt, at lufthavnens samarbejde med konkrete virksomheder potentielt vil kunne medføre, at lufthavnen medvirker til udvikling af materiel til militært brug og om ministeren set i lyset af samarbejdet mellem Boeing og de odenseanske våbenfabrikker om militære våbensystemer kan bekræfte, at test- og valideringsfaciliteten udelukkende vil blive benyttet til civile og ikke militære formål.', N'Spm. om ministeren kan oplyse, om han finder det hensigtsmæssigt, at lufthavnens samarbejde med konkrete virksomheder potentielt vil kunne medføre, at lufthavnen medvirker til udvikling af materiel til militært brug m.v., til transportministeren, kopi til forsvarsministeren', 16183, N'', N'', null, N'2015-12-06 05:36:08.537');
