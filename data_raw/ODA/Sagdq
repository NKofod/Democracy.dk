INSERT INTO ODA.dbo.Sag (id, typeid, kategoriid, statusid, titel, titelkort, offentlighedskode, nummer, nummerprefix, nummernumerisk, nummerpostfix, resume, afstemningskonklusion, periodeid, afgørelsesresultatkode, baggrundsmateriale, opdateringsdato, statsbudgetsag, begrundelse, paragrafnummer, paragraf, afgørelsesdato, afgørelse, rådsmødedato, lovnummer, lovnummerdato, retsinformationsurl, fremsatundersagid, deltundersagid) VALUES (43337, 10, 11, 20, N'Vil ministeren kommentere de tilfælde, som har været omtalt i pressen, hvor visse kommuner har rejst erstatningsansvar over for de efterladte til en plejehjemsbeboer, fordi plejeboligen efterfølgende har været i dårlig stand?', N'Om erstatningskrav over for de efterladte til en plejehjemsbeboer.', N'O', N'S 6220', N'S', N'6220', N'', N'', null, 22, null, null, N'2017-12-07 18:22:36.727', 0, N'I løbet af sommeren har kommuner flere gange gjort erstatningskrav gældende imod et dødsbo eller de efterladte til en ældre, der har været visiteret til plejebolig. Derfor mener spørgeren, at ministeren bør se nærmere på disse sager. 
Særligt et tilfælde fra Hørning, hvor kommunen rejste erstatningskrav på over 30.000 kr., vækker harme. Det virker urimeligt, at en kommune kan kræve erstatningsansvar imod de efterladte eller imod boet for eksempelvis gulvskader. Når skader i det omfang overhovedet kan opstå, vidner det om, at kommunens opsyn med den ældre ikke har været tilstrækkeligt. Spørgeren har meget svært ved at forstå, at familien eller boet skal betale for dette. Tværtimod burde det være noget, som kommunen fik repareret på deres regning. Skaderne ville jo netop ikke være opstået, hvis kommunen havde efterlevet dens rimelige opsyn med de ældre og med de lejligheder, der stilles til rådighed. 
Når en kommune visiterer til plejehjem, kan den ældre oftest ikke længere tage vare på sig selv. Naturligvis varierer det, hvor svagelige de ældre er, når de kommer på plejehjem. Men grundlæggende må det være sådan, at kommunen overtager ansvaret for den daglige pleje. Det må efter spørgerens mening også gælde, at selve lejligheden skal være under opsyn. Det handler jo om, at vore ældre skal have en tryg og værdig tilværelse - både før og efter de kommer på plejehjem. Derfor bør kommunen ikke blot kunne henvise til lejelovens regler. 
Det her handler nemlig ikke bare om jura. Det handler om menneskelig anstændighed - både over for de ældre og over for de efterladte, der har lidt et svært tab. Derfor må der naturligvis gælde særlige regler for plejehjem og for ældre, der visiteres til plejehjem. Disse regler bør ikke nødvendigvis være de samme, som gælder for almindelige lejeboliger. I de nævnte tilfælde bør reparationen i stedet betales af kommunens vedligeholdelseskonto.', null, null, null, null, null, null, null, null, null, null);
