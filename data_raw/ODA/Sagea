INSERT INTO ODA.dbo.Sag (id, typeid, kategoriid, statusid, titel, titelkort, offentlighedskode, nummer, nummerprefix, nummernumerisk, nummerpostfix, resume, afstemningskonklusion, periodeid, afgørelsesresultatkode, baggrundsmateriale, opdateringsdato, statsbudgetsag, begrundelse, paragrafnummer, paragraf, afgørelsesdato, afgørelse, rådsmødedato, lovnummer, lovnummerdato, retsinformationsurl, fremsatundersagid, deltundersagid) VALUES (49470, 5, 15, 13, N'Forslag om folketingsbeslutning om forsøg med lægeligt ordineret heroin til særlig hårdt belastede stofmisbrugere.', N'Forsøg med lægeligt ordineret heroin til særlig hådt belastede stofmisbrugere', N'O', N'B 143', N'B', N'143', N'', N'Forslaget betød, at der skulle etableres et forsøg med lægeligt ordineret heroin til særlig hårdt belastede stofmisbrugere efter en række nærmere fastsatte kriterier for forsøgets rammer og indhold.
Forslagstillerne mente, at der var behov for nye tiltag for at forbedre situationen for navnlig de dårligst stillede stofmisbrugere, og henviste til, at Danmark på trods af en økonomisk opprioritering af behandlingsindsatsen over en årrække fortsat har en lige så høj dødelighed som i 1995.
Forslagsstillerne understregede, at lægeordineret heroin ikke skulle erstatte forebyggelse eller behandling, men skulle ses som et element i et samlet spekter af midler, der kunne anvendes i stofmisbrugsbehandlingen.
Forslagsstillerne henviste endvidere til, at en række europæiske lande allerede havde opnået gode erfaringer med lægeordineret heroin, men anerkendte, at sociokulturelle forskelle landene imellem betød, at Danmark ikke blot kunne overføre erfaringerne fra udlandet, men måtte iværksætte sit eget forsøg.', null, 23, null, null, N'2017-12-05 23:31:39.490', 0, null, null, null, null, null, null, null, null, null, null, null);
