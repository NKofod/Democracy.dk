INSERT INTO ODA.dbo.Sag (id, typeid, kategoriid, statusid, titel, titelkort, offentlighedskode, nummer, nummerprefix, nummernumerisk, nummerpostfix, resume, afstemningskonklusion, periodeid, afgørelsesresultatkode, baggrundsmateriale, opdateringsdato, statsbudgetsag, begrundelse, paragrafnummer, paragraf, afgørelsesdato, afgørelse, rådsmødedato, lovnummer, lovnummerdato, retsinformationsurl, fremsatundersagid, deltundersagid) VALUES (48551, 10, 11, 20, N'Finder ministeren, at det er rimeligt, at man på CVU-Øresunds ergoterapeutuddannelse har afviklet en intern prøve i sygdomslære uden at afgrænse det pensum, som de studerende skulle testes i ud over at meddele eleverne, at der var tale om »kendt stof«, og finder ministeren det ikke problematisk at afvikle multiplechoice-prøver, der jo kræver en meget præcis viden, inden for fag, hvor der ikke er givet et præcist afgrænset pensum?', N'Om På CVU-Øresund', N'O', N'S 3149', N'S', N'3149', N'', N'', null, 23, null, null, N'2017-12-05 23:26:04.233', 0, N'På CVU-Øresunds ergoterapeutuddannelse i København har man afviklet en intern prøve i sygdomslære. Prøven har karakter af en stopprøve, da den skal bestås for at kunne forblive på det semester, som den studerende befinder sig på. 

De studerende har alene fået oplyst, at de skal prøves i »kendt stof«. Pensum har altså ikke været afgrænset. Den undervisning, de studerende har modtaget i sygdomslære, er blevet afviklet ni måneder før prøven finder sted. I de mellemliggende ni måneder har der ingen undervisning været i faget. 

Faget har ikke noget pensum, og de studerende har behandlet forskellige emner i forløbet og har således kun opnået en begrænset fælles referenceramme.

Spørgeren finder, at formålet med enhver eksamen må være at teste de studerendes kundskaber. Når der vælges en prøveform som multiple-choice, er der behov for en stor fælles referencramme og en afgrænsning af, hvad den studerende kan stilles til regnskab. For at kunne teste kundskaber meningsfyldt er det nødvendigt, at den studerende har mulighed for at vide, hvad det er den pågældende skal testes i.

Spørgeren finder det derfor påfaldende og mærkværdigt, at man på CVU-Øresund har valgt ikke at fastlægge et pensum til den pågældende eksamen, og således reelt har gjort det umuligt for de studerende at vide, hvad de skulle testes i ud over betegnelsen »kendt stof«, som mildt sagt er en upræcis afgrænsning.

Spørgeren ønsker ministerens vurdering af, hvorvidt en sådan prøveform er acceptabel og om ministerens vurdering af, hvorvidt en prøve, der tester elevernes viden så konkret og præcist som en multiple-choice, ikke kræver en afgræsning af stofområdet i form af et pensum, herunder om ministeren vil sørge for at tage kontakt til CVU-Øresund og indgå i dialog med institutionen i forhold til at stille eleverne til regnskab for et afgrænset pensum.', null, null, null, null, null, null, null, null, null, null);
