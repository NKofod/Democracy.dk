INSERT INTO ODA.dbo.Dokument (id, typeid, kategoriid, statusid, offentlighedskode, titel, dato, modtagelsesdato, frigivelsesdato, paragraf, paragrafnummer, spørgsmålsordlyd, spørgsmålstitel, spørgsmålsid, procedurenummer, grundnotatstatus, dagsordenudgavenummer, opdateringsdato) VALUES (892183, 17, 22, 1, N'O', N'SUU alm. del - svar på spm. 575 om redegørelse for Sundhedsstyrelsens plan for udrulning af vaccination mod COVID-19 til gadehjemløse og herbergsbeboere, fra sundhedsministeren', N'2021-02-03 00:00:00.000', N'2021-02-03 00:00:00.000', N'2021-02-03 16:10:00.000', N'', N'', N'Vil ministeren redegøre for Sundhedsstyrelsens plan for udrulning af vaccination mod COVID-19 til gadehjemløse og herbergsbeboere? Er gadehjemløse uden aktuelle patientforløb omfattet af en af de af Sundhedsstyrelsen definerede grupper? Er der en plan for, hvornår hjemløse tilknyttet væresteder og herberger skal vaccineres?
Det fremgår af Sundhedsstyrelsens retningslinjer for håndtering af vaccination mod COVID, at der er en prioriteret rækkefølge på 12 grupper. Det er uklart, hvor hjemløse – henholdsvis gadesovere og herbergsbeboere - er indtænkt i denne plan. En række af disse hjemløse modtager ikke hjælp efter andre servicelovsbestemmelser end §§ 110 eller 85, og en del modtager slet ingen hjælp og bor samtidig på gaden og bevæger sig derfor op mod 24 timer i døgnet i det offentlige rum, hvor de potentielt er udsat for smitte. Vi ved, at gruppen har en række fysiske sygdomme og derfor må formodes at være særligt udsatte i forbindelse med et evt. COVID-forløb, men de er ikke nødvendigvis i behandling og vil derfor ikke fremgå af regionernes opgørelser over personer med særlig risiko for smitte. Spørger er bevidst om, at hjemløse i Danmark først er blevet ramt af epidemien sent, men meldinger fra diverse organisationer lyder nu, at gadehjemløse og herbergsbeboere har været hårdt ramt over julen og nytåret, og at smitte i disse miljøer er svære at inddæmme, da de potentielt smittede bevæger sig meget rundt og har en række sociale udfordringer.   
', N'Spm. om redegørelse for Sundhedsstyrelsens plan for udrulning af vaccination mod COVID-19 til gadehjemløse og herbergsbeboere, til sundheds- og ældreministeren', 885848, N'', N'', null, N'2021-02-12 13:08:17.133');
