INSERT INTO ODA.dbo.Sag (id, typeid, kategoriid, statusid, titel, titelkort, offentlighedskode, nummer, nummerprefix, nummernumerisk, nummerpostfix, resume, afstemningskonklusion, periodeid, afgørelsesresultatkode, baggrundsmateriale, opdateringsdato, statsbudgetsag, begrundelse, paragrafnummer, paragraf, afgørelsesdato, afgørelse, rådsmødedato, lovnummer, lovnummerdato, retsinformationsurl, fremsatundersagid, deltundersagid) VALUES (14057, 7, null, 54, N'Forslag til Europa-Parlamentets og Rådets direktiv om ændring af direktiv 2000/25/EF for så vidt angår bestemmelserne for traktorer, der markedsføres under fleksibilitetsordningen', N'Direktivforslag om bestemmelserne for traktorer, der markedsføres under fleksibilitetsordningen', N'O', N'KOM (2010) 0607', N'KOM', N'607', N'', N'<strong>Forslaget er modtaget</strong> af Folketinget i dansk sprogudgave den&nbsp;28.&nbsp;oktober&nbsp;2010, jf. <a href="http://www.euo.dk/upload/application/pdf/d83cf832/Protokol2_naerhed.pdf">Lissabontraktatens protokol nr. 2</a> om kontrol med nærhedsprincippet. <strong>Fristen </strong>på 8 uger for fremsendelse af Folketingets begrundede udtalelse <strong>udløb den&nbsp;23.&nbsp;december&nbsp;2010.<br />
<br />
</strong><a href="/138538.docid">Læs</a> Kommissionens fremsendelsesbrev <br />
<br />
Tilknyttede dokumenter: SEK (2010) 1251 og SEK (2010) 1252<br />
<br />
Det gældende direktiv regulerer udstødningsemissionerne (kulilte (CO), kulbrinter (HC), kvælstofoxider (NOx) og partikler (PM)) fra dieselmotorer monteret på landbrugs- og skovbrugstraktorer.&nbsp; <br />
<br />

<p style="text-align: left;">
Direktivet indeholder bestemmelser om stadig strengere emissionsgrænseværdier med dertil hørende datoer for overholdelse af kravene. Fabrikanterne skal sikre, at nye motorer opfylder kravene til emissionsgrænseværdier, før de kan markedsføres. 
</p>

<p style="text-align: left;">
På grundlag af direktiv 2004/26/EF blev der ved direktiv 2005/13/EF indført det gældende trin for emissionsgrænseværdier, der finder anvendelse på størstedelen af alle dieselmotorer og benævnes trin III A. Disse grænseværdier vil fra den 1. januar 2011 efterhånden blive erstattet med de strengere trin III B-grænseværdier for nye traktorer, der sælges. Fra den 1. januar 2010 og fremefter startede typegodkendelsesperioden for disse motorer. Dette vedrører en lang række forskellige traktorer i produktion. 
</p>

<p style="text-align: left;">
Målet med forslaget er at foretage følgende ændringer af direktiv 2000/25/EF: Andelen af motorer, der kan markedsføres under fleksibilitetsordningen i hver enkelt motorkategori, øges fra 20 til 50 % af traktorfabrikanternes årlige salg af udstyr, og det maksimale antal motorer, der som et valgfrit alternativ kan markedsføres under&nbsp; fleksibilitetsordningen, justeres i overgangsperioden mellem trin III A- og trin III Bemissionsgrænseværdierne. Disse foranstaltninger udløber den 31. december 2013. <br />
<br />
Resumeet er udarbejdet af Folketingets EU-Oplysning. For yderligere notater m.v. om forslaget se boksen med &quot;relaterede dokumenter&quot;. 
</p>', null, 11, null, null, N'2015-12-03 07:40:31.440', 0, null, null, null, null, null, null, null, null, null, null, null);
