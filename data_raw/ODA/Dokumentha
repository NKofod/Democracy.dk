INSERT INTO ODA.dbo.Dokument (id, typeid, kategoriid, statusid, offentlighedskode, titel, dato, modtagelsesdato, frigivelsesdato, paragraf, paragrafnummer, spørgsmålsordlyd, spørgsmålstitel, spørgsmålsid, procedurenummer, grundnotatstatus, dagsordenudgavenummer, opdateringsdato) VALUES (179596, 17, 22, 1, N'O', N'EUU alm. del - svar på spørgsmål 82, fra udenrigsministeren', N'2011-01-26 00:00:00.000', N'2011-01-26 00:00:00.000', N'2011-01-26 13:13:25.000', N'', N'', N'Ministeren bedes - i forlængelse af svaret på EUU alm. del - spørgsmål 11 - oplyse, om regeringen vil ændre kravene til medfinansiering af regionalfondsprojekter, så det ikke alene er offentlige myndigheder og offentlige institutioner, der i praksis er de eneste, som er sikret fri og lige adgang til disse midler? Ministeren bedes desuden oplyse, om regeringen vil indføre regler, hvor ikke-offentlige midler kan anvendes til medfinansiering af regionalfondsprojekter?
Spørgsmålet skal ses i lyset af ministerens svar på EUU alm. del - spørgsmål 11, hvor ministeren oplyser, at ”alle fysiske og juridiske personer har adgang til søge om strukturfondsmidler i Danmark. Det gælder således også almennyttige non-profit aktører”. Hermed har ministeriet fastslået en lige og fri adgang til ansøgning.
Men i praksis forvaltes midlerne således, at de 50 pct. medfinansiering alene kan komme fra offentlige myndigheder og offentlige institutioner, eller hvis en aktør har modtaget et andet offentligt tilskud til at medfinansiere med. Det forekommer uforståeligt, at private midler fra f.eks. almennyttige non-profit NGO’er ikke har samme værdi som medfinansiering, idet det ville kunne øge antallet af projekter, men også styrke civilsamfundets og den private sektors medansvar og medfinansiering af den regionale udvikling.', N'Spm. om regeringen vil ændre kravene til medfinansiering af regionalfondsprojekter, så det ikke alene er offentlige myndigheder og offentlige institutioner, der i praksis er de eneste, som er sikret fri og lige adgang til disse midler, til udenrigsministeren, kopi til økonomi- og erhvervsministeren', 179595, N'', N'', null, N'2015-12-06 01:44:18.703');
