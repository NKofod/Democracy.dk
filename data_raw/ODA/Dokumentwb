INSERT INTO ODA.dbo.Dokument (id, typeid, kategoriid, statusid, offentlighedskode, titel, dato, modtagelsesdato, frigivelsesdato, paragraf, paragrafnummer, spørgsmålsordlyd, spørgsmålstitel, spørgsmålsid, procedurenummer, grundnotatstatus, dagsordenudgavenummer, opdateringsdato) VALUES (510696, 17, 22, 3, N'O', N'REU alm. del - svar på spm. 512, fra justitsministeren', N'2008-04-23 00:00:00.000', N'2008-04-23 00:00:00.000', N'2008-04-23 15:10:27.607', N'', N'', N'Giver de seneste eksempler på knægtelse af offentligt ansattes ytringsfrihed i f.eks. Gentofte og Samsø Kommuner ministeren anledning til påny at præcisere offentligt ansattes ytringsfrihed i forhold til de kommunale arbejdsgivere, - ledere og - mellemledere?   

Der henvises til, at VK-regeringen ved flere lejligheder har erklæret sig enig i de konklusioner og anbefalinger, der blev udtalt i betænkning 1472/06 om offentligt ansattes ytringsfrihed, og også initieret en oplysningskampagne desangående i  samarbejde med KL. På trods heraf kan man konstatere, at ytringsfriheden stadig har trange kår i nogle kommuner, jf. de seneste eksempler i Samsø og Gentofte Kommuner. Derfor vil spørgeren gerne høre, hvorvidt ministeren føler anledning til at følge op med yderligere oplysningskampagner over for kommunerne, så vi dels sikrer, at der ikke er børn, som kommer i klemme, dels at kommuner ikke giver deres ansatte mundkurv på.
', N'Spm. om knægtelse af offentligt ansattes ytringsfrihed, til justitsministeren', 510695, N'', N'', null, N'2015-12-05 17:44:31.083');
