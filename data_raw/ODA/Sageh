INSERT INTO ODA.dbo.Sag (id, typeid, kategoriid, statusid, titel, titelkort, offentlighedskode, nummer, nummerprefix, nummernumerisk, nummerpostfix, resume, afstemningskonklusion, periodeid, afgørelsesresultatkode, baggrundsmateriale, opdateringsdato, statsbudgetsag, begrundelse, paragrafnummer, paragraf, afgørelsesdato, afgørelse, rådsmødedato, lovnummer, lovnummerdato, retsinformationsurl, fremsatundersagid, deltundersagid) VALUES (53844, 10, 11, 20, N'Finder ministeren det rimeligt, at en landsretsafgørelse, der erklærer sager ved Huslejenævnet ugyldige, ikke fører til, at sagsøger stilles, som om nævnsafgørelserne ikke var afsagt, og hvad vil ministeren foretage sig for at sikre, at landets Huslejenævn fremover retter sig efter domstolsafgørelser?', N'Om sager ved Huslejenævnet', N'O', N'S 690', N'S', N'690', N'', N'', null, 25, null, null, N'2017-12-05 22:25:11.790', 0, N'Ved Vestre Landsrets afgørelse af 7. april 2006 (U2006.2149V) slår landsretten fast, at sager ved Randers Huslejenævn, hvor en række medlemmer efter retsplejelovens § 62, stk. 1, burde have veget sit sæde, er ugyldige, idet kravene om habilitet er overtrådt.

Den normale konsekvens af ugyldighed er negativ kontraktsinteresse, ifølge hvilken parterne skal stilles, som om den pågældende beslutning aldrig er truffet. Efter den afsagte landsretsdom har Randers Huslejenævn imidlertid ikke foretaget sig det nødvendige, for at stille udlejere som udlejere var, inden de ugyldige afgørelser blev truffet. Derimod har man over for udlejere, hvis repræsentant var sagsøger og vinder af nævnte retssag, gjort gældende, at de, såfremt de anfægter resultatet af de ugyldige afgørelser, på ny må anlægge sag herom ved Randers Huslejenævn.

For spørgeren virker det ikke bare besynderligt, men også for et retssamfund uønskeligt, at en vindende part ved Vestre Landsret for at gøre ugyldigheden gældende skal anlægge sag ved Huslejenævnet for at få dette nævn til at vurdere, om det materielle indhold i sagen burde have ført til den afgørelse, som af landsretten ud fra formelle betingelser er erklæret ugyldig. I et retssamfund må ugyldighed føre til, at nævnsafgørelserne betragtes som ikke eksisterende. Skulle nogle lejere da fortsat mene at have et krav mod deres udlejere, må disse på ny anlægge en sag ved Huslejenævnet. 

Skulle Huslejenævnet i disse sager komme frem til en stadfæstelse, må en eventuel merudgift for lejerne føre til et erstatningskrav mod Huslejenævnet, subsidiært Randers Kommune, idet habilitet er retsstridig, jf. Vestre Landsrets dom samt retsplejeloven, ligesom der mellem de lidte tab og de retsstridige nævnsafgørelser må antages at være kausalitet og adækvans, forårsaget af en - af de inhabile medlemmer udvist - culpøs adfærd. Betingelserne for erstatningsansvar er således til stede.

På denne baggrund bedes ministeren tage stilling til, om hun finder det rimeligt, at ugyldighed af Randers Huslejenævn ikke fører til, at udlejer og lejer stilles, som om sagerne ikke var behandlet, samt hvad hun agter at foretage sig for at sikre såvel lejeres som udlejeres retssikkerhed, når domstolsafgørelser går Huslejenævnet imod.', null, null, null, null, null, null, null, null, null, null);
