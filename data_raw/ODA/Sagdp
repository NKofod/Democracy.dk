INSERT INTO ODA.dbo.Sag (id, typeid, kategoriid, statusid, titel, titelkort, offentlighedskode, nummer, nummerprefix, nummernumerisk, nummerpostfix, resume, afstemningskonklusion, periodeid, afgørelsesresultatkode, baggrundsmateriale, opdateringsdato, statsbudgetsag, begrundelse, paragrafnummer, paragraf, afgørelsesdato, afgørelse, rådsmødedato, lovnummer, lovnummerdato, retsinformationsurl, fremsatundersagid, deltundersagid) VALUES (43299, 10, 11, 20, N'Er der siden juli 2003 lavet nye undersøgelser af sikkerheden på skolernes legepladser, og hvis ja, viser disse så en forbedring af sikkerheden på skolernes legepladser?', N'Om sikkerheden på skolernes legepladser.', N'O', N'S 6190', N'S', N'6190', N'', N'', null, 22, null, null, N'2017-12-07 18:22:29.577', 0, N'Spørgeren har tidligere stillet et § 20-spørgsmål (S 5868), hvori der spørges til sikkerheden på skolernes legepladser. Spørgeren har modtaget et svar, der overhovedet ikke svarer på det, der spørges om, men hvor ministeren svarer, at han ikke er bekendt med stikprøver foretaget af Forbrugerinformation af sikkerheden på skolernes legepladser. 
Til orientering kan det oplyses, at spørgeren i første omgang sendte sit § 20-spørgsmål til familie- og forbrugerministern, men at spørgeren blev kontaktet af ministeriet, der gerne ville viderestille spørgsmålet til undervisningsministeren, da spørgsmålet drejede sig om sikkerheden på skolernes legepladser.
Den stikprøve, der omtaltes, er fra Forbrugerinformation, og den viste, at andelen af alvorlige sikkerhedsfejl, som f.eks. kvælningsrisiko, er vokset. Samtidig er det ifølge statistikkerne i skolegården, at antallet af ulykker stiger mest.
Forbrugerinformationens undersøgelse er gennemført af certificeret legepladsinspektør Annelise Bunch, og man kan læse om den på Forbrugerstyrelsens hjemmeside.
Undervisningsministerens kollega familie- og forbrugermisteren nedsatte i sep. 2004 en arbejdsgruppe, der skulle se på forbedring af sikkerheden på landets legepladser - herunder også skolernes.
Den rapport kom til ministerens orientering i september 2005.
Da spørgeren finder der yderst relevant at spørge regeringen, hvordan det seneste billede er af sikkerheden på vore legepladser - herunder dem der ligger på skolerne, håber spørgeren, at ministeren vil undersøge sagen og svare på det stillede spørgsmål i stedet for blot at svare, at ministeren ikke er bekendt med resultatet af de omtalte stikprøver, og at Undervisningsministeriet har udstedt en bekendtgørelse om tilsyn med folkeskolens elever i skoletiden, hvoraf fremgår, at kommunalbestyrelsen skal dage omsorg for, at legepladser mv. er indrettet på en sådan måde, at der ikke er særlige faremomenter til stede.', null, null, null, null, null, null, null, null, null, null);
