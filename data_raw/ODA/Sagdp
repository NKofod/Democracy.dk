INSERT INTO ODA.dbo.Sag (id, typeid, kategoriid, statusid, titel, titelkort, offentlighedskode, nummer, nummerprefix, nummernumerisk, nummerpostfix, resume, afstemningskonklusion, periodeid, afgørelsesresultatkode, baggrundsmateriale, opdateringsdato, statsbudgetsag, begrundelse, paragrafnummer, paragraf, afgørelsesdato, afgørelse, rådsmødedato, lovnummer, lovnummerdato, retsinformationsurl, fremsatundersagid, deltundersagid) VALUES (43259, 10, 11, 20, N'Vil udenrigsministeren i forlængelse af spørgsmål nr. S 5207 og S 5671 redegøre for, om det af Enhedslisten stillede ændringsforslag til B 139 medfører, at Danmark påtager sig et juridisk ansvar, der er uforeneligt med internationale konventioner?', N'Om dansk militær bidrag i Irak.', N'O', N'S 6152', N'S', N'6152', N'', N'', null, 22, null, null, N'2017-12-07 18:22:23.027', 0, N'Enhedslistens ændringsforslag til B 139 (forlængelsen af de danske tropper i Irak) lød: 

»I forbindelse med videreførelsen af dansk militært bidrag i Irak vil Danmark nægte at deltage i fælles operationer, hvis der ikke er garanti for, at fangerne ikke bliver udsat for tortur, forsvinden, død eller andre menneskerettighedskrænkelser. Derfor skal de danske styrker føre eller modtage fortegnelser over, hvem disse fanger er (navn) og hvor de befinder sig indtil løsladelsen eller domfældelsen«.

I debatten om ændringsforslaget sagde forsvarsministeren: 

»&vi kan kun handle inden for de begrænsninger, der følger af folkeretten. Og Enhedslistens forslag går ud over disse begrænsninger. 
    Det vil således ikke være i overensstemmelse med folkeretten, hvis Danmark ved at kræve garantier eller lister over tilbageholdte påtog sig et juridisk ansvar for personer, der enten er i irakisk varetægt eller i varetægt hos et andet koalitionsland«.

Det fremgår dog ikke af dette svar, om forsvarsministeren mener, at Enhedslistens konkrete ændringsforslag er uforeneligt med internationale konventioner. 
Svaret kan forstås som om, forsvarsministeren svarer hypotetisk og/eller generelt - og ikke konkret i forhold til det specifikke ændringsforslag stillet af Enhedslisten til B 139.

Som det fremgår af debatten ved 2. behandlingen mener spørgeren ikke, der er tale om, at Danmark påtager sig et juridisk ansvar, der er uforeneligt med internationale konventioner.', null, null, null, null, null, null, null, null, null, null);
