INSERT INTO ODA.dbo.Dokument (id, typeid, kategoriid, statusid, offentlighedskode, titel, dato, modtagelsesdato, frigivelsesdato, paragraf, paragrafnummer, spørgsmålsordlyd, spørgsmålstitel, spørgsmålsid, procedurenummer, grundnotatstatus, dagsordenudgavenummer, opdateringsdato) VALUES (547931, 17, 22, 1, N'O', N'FIU - Svar på § 20 spm. 2 fra finansministeren', N'2009-04-03 00:00:00.000', N'2009-04-03 00:00:00.000', N'2009-04-03 11:09:46.000', N'Ministeriet for Børn, Undervisning og Ligestilling', N'20', N'Ad Statsrevisorernes beretning 6/2008 om forvaltning af tilskuddet til Sydslesvig

I statsrevisorernes bemærkning til beretning hedder det bl.a., at en så omfattende tilskudsordning, som den der vedrører Sydslesvig, bør have hjemmel i lov.

Af anmærkningerne til tekstanmærkning 104 ad § 20.84.01. på Finansloven for 2009, der er den eneste hjemmel til fordeling af bevillingen på 431,5 mio. kr. til Danske Kulturelle anliggender i Sydslesvig fremgår, at tekstanmærkningen er indsat i 1995, altså for 14 år siden.

I Finansudvalgets beretning nr. 1 af 27. oktober 2005, pkt. 5.2, hedder det bl.a., at 

antallet af tekstanmærkninger bør begrænses mest muligt og 
at tekstanmærkninger af materiel art ikke bør opføres uændrede på finansloven over en årrække, men i stedet enten bør indarbejdes i gældende materiel lov. 
Finansudvalget noterede sig i denne forbindelse i beretningen, at Finansministeriet havde tilkendegivet, at ministeriet vil foranledige, at de tekstanmærkninger der findes på finansloven af denne art, vil blive søgt indarbejdet, når lejlighed bød sig.

På denne baggrund ønskes oplyst, 

Om finansministeren er enig med statsrevisorerne i, at tilskudsordningen vedrørende Sydslesvig bør have hjemmel i materiel lov og ikke kun på finansloven? 
Om finansministeren på baggrund af disse principielle overvejelser vil tage initiativ til – i samarbejde med Undervisningsministeriet - at tilvejebringe en sådan materiel lovhjemmel ud over den bevillingsmæssige lovhjemmel på finansloven? ', N'Spm. om antallet af tekstanmærkninger bør begrænses mest muligt og at tekstanmærkninger af materiel art ikke bør opføres uændrede på finansloven over en årrække, men i stedet enten bør indarbejdes i gældende materiel lov, til finansministeren', 547930, N'', N'', null, N'2015-12-05 20:38:15.377');
