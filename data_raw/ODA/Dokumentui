INSERT INTO ODA.dbo.Dokument (id, typeid, kategoriid, statusid, offentlighedskode, titel, dato, modtagelsesdato, frigivelsesdato, paragraf, paragrafnummer, spørgsmålsordlyd, spørgsmålstitel, spørgsmålsid, procedurenummer, grundnotatstatus, dagsordenudgavenummer, opdateringsdato) VALUES (477344, 17, 30, 3, N'O', N'REU alm. del - Foreløbigt svar på spm. 153, fra justitsministeren', N'2007-05-03 00:00:00.000', N'2007-05-03 00:00:00.000', N'2007-05-08 09:32:52.827', N'', N'', N'Har den dokumentation, som Retsudvalget den 17. august 2006 fik fremlagt af repræsentant fra firmaet Pantektor AB, jf. REU, 2005-06, alm. del – bilag 771, hvori det blev belyst, at en række brande på færgen Scandinavian Star blev påsat efter at den påståede brandstifter, Erik Mørk Andersen, var omkommet, givet anledning til, at anklagemyndigheden foranlediger efterforskningen af mordbranden på Scandinavian Star genoptaget?

Der henvises til, at det af Pantektor AB''s dokumentation fremgår, at en eller flere ombordværende med skibsteknisk kendskab påsatte brandene på Scandinavian Star, dels at der ikke kan være tale om Erik Mørk Andersen, som hidtil har været udpeget som brandstifter. Da den eller de reelle brandstiftere har
haft skibskendskab og derved burde have indset, at deres ildspåsættelser kunne udsætte andres liv for fare, vil der være tale om en overtrædelse af straffelovens § 180, og dermed et forhold, der ikke kan forældes. Ifølge retsplejelovens § 96 er det de offentlige anklageres opgave sammen med
politiet at fremme enhver sag med den hurtighed, som sagens beskaffenhed tillader og derved bl.a. tilsikre, at strafskyldige drages til ansvar. Da dokumentationen blev fremlagt i Retsudvalget for et halvt år siden, må det forventes, at de offentlige anklagere med politiet har iværksat undersøgelse af påstandene og eventuelt foranlediget en ny efterforskning.
', N'Spm. om Scandinavian Star-sagen, til justitsministeren', 477343, N'', N'', null, N'2015-12-05 15:29:36.530');
INSERT INTO ODA.dbo.Dokument (id, typeid, kategoriid, statusid, offentlighedskode, titel, dato, modtagelsesdato, frigivelsesdato, paragraf, paragrafnummer, spørgsmålsordlyd, spørgsmålstitel, spørgsmålsid, procedurenummer, grundnotatstatus, dagsordenudgavenummer, opdateringsdato) VALUES (477345, 17, 22, 3, N'O', N'REU alm. del - svar på spørgsmål 153, fra justitsministeren', N'2007-05-29 00:00:00.000', N'2007-05-30 00:00:00.000', N'2007-05-30 16:46:59.000', N'', N'', N'Har den dokumentation, som Retsudvalget den 17. august 2006 fik fremlagt af repræsentant fra firmaet Pantektor AB, jf. REU, 2005-06, alm. del – bilag 771, hvori det blev belyst, at en række brande på færgen Scandinavian Star blev påsat efter at den påståede brandstifter, Erik Mørk Andersen, var omkommet, givet anledning til, at anklagemyndigheden foranlediger efterforskningen af mordbranden på Scandinavian Star genoptaget?

Der henvises til, at det af Pantektor AB''s dokumentation fremgår, at en eller flere ombordværende med skibsteknisk kendskab påsatte brandene på Scandinavian Star, dels at der ikke kan være tale om Erik Mørk Andersen, som hidtil har været udpeget som brandstifter. Da den eller de reelle brandstiftere har
haft skibskendskab og derved burde have indset, at deres ildspåsættelser kunne udsætte andres liv for fare, vil der være tale om en overtrædelse af straffelovens § 180, og dermed et forhold, der ikke kan forældes. Ifølge retsplejelovens § 96 er det de offentlige anklageres opgave sammen med
politiet at fremme enhver sag med den hurtighed, som sagens beskaffenhed tillader og derved bl.a. tilsikre, at strafskyldige drages til ansvar. Da dokumentationen blev fremlagt i Retsudvalget for et halvt år siden, må det forventes, at de offentlige anklagere med politiet har iværksat undersøgelse af påstandene og eventuelt foranlediget en ny efterforskning.
', N'Spm. om Scandinavian Star-sagen, til justitsministeren', 477343, N'', N'', null, N'2015-12-05 15:43:15.847');
