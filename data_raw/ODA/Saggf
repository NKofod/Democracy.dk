INSERT INTO ODA.dbo.Sag (id, typeid, kategoriid, statusid, titel, titelkort, offentlighedskode, nummer, nummerprefix, nummernumerisk, nummerpostfix, resume, afstemningskonklusion, periodeid, afgørelsesresultatkode, baggrundsmateriale, opdateringsdato, statsbudgetsag, begrundelse, paragrafnummer, paragraf, afgørelsesdato, afgørelse, rådsmødedato, lovnummer, lovnummerdato, retsinformationsurl, fremsatundersagid, deltundersagid) VALUES (86036, 7, null, 54, N'RAPPORT FRA KOMMISSIONEN TIL EUROPA-PARLAMENTET OG RÅDET om den overordnede gennemførelse af offentlig kontrol i medlemsstaterne (2017-2018) med henblik på at sikre verifikation af overholdelsen af fødevare- og foderlovgivningen og reglerne for dyresundhed og dyrevelfærd {SWD(2020) 283 final}', N'RAPPORT FRA KOMMISSIONEN om offentlig kontrol i medlemsstaterne (2017-2018) om fødevare- og dyrevelfærdslovgivning', N'O', N'KOM (2020) 0756', N'KOM', N'756', N'', N'', null, 152, null, null, N'2020-11-25 14:46:00.860', 0, null, null, null, null, null, null, null, null, null, null, null);
INSERT INTO ODA.dbo.Sag (id, typeid, kategoriid, statusid, titel, titelkort, offentlighedskode, nummer, nummerprefix, nummernumerisk, nummerpostfix, resume, afstemningskonklusion, periodeid, afgørelsesresultatkode, baggrundsmateriale, opdateringsdato, statsbudgetsag, begrundelse, paragrafnummer, paragraf, afgørelsesdato, afgørelse, rådsmødedato, lovnummer, lovnummerdato, retsinformationsurl, fremsatundersagid, deltundersagid) VALUES (86062, 10, 11, 4, N'Hvad er ministerens holdning til udviklingen i antallet af unge, danske rygere under 18 år i perioden fra 2000 til 2020?', N'Om antallet af unge, danske rygere under 18 år.', N'O', N'S 445', N'S', N'445', N'', N'', null, 153, null, null, N'2020-11-27 11:23:54.487', 0, N'Ingen.', null, null, null, null, null, null, null, null, null, null);
INSERT INTO ODA.dbo.Sag (id, typeid, kategoriid, statusid, titel, titelkort, offentlighedskode, nummer, nummerprefix, nummernumerisk, nummerpostfix, resume, afstemningskonklusion, periodeid, afgørelsesresultatkode, baggrundsmateriale, opdateringsdato, statsbudgetsag, begrundelse, paragrafnummer, paragraf, afgørelsesdato, afgørelse, rådsmødedato, lovnummer, lovnummerdato, retsinformationsurl, fremsatundersagid, deltundersagid) VALUES (86063, 10, 11, 20, N'Mener ministeren, at en dansk model for rejsevejledninger bør fastsættes på baggrund af en forudgående sundhedsfaglig vurdering af nødvendigheden af per automatik at fraråde rejser så snart et land eller region registrerer mere end et på forhånd fastsat antal nye smittetilfælde pr. 100.000 indbyggere pr. uge, samt at de pågældende grænseværdier bør fastsættes efter anbefaling fra sundhedsfaglige eksperter?', N'Om en dansk model for rejsevejledninger.', N'O', N'S 446', N'S', N'446', N'', N'', null, 153, null, null, N'2020-12-04 08:46:26.613', 0, N'Regeringen har den 19. november 2020 meldt ud, at den hidtidige model for rejsevejledninger, der blev fastsat tilbage i juni måned, fortsætter, dog med en øget regional opdeling. Udenrigsministeren udtalte, at den øgede regionale tilgang ikke på nuværende tidspunkt vil medføre nogen ændring, da smittetrykket i alle europæiske regioner er højere end de fastsætte grænseværdier. 
Regeringen har for blot få uger siden lanceret det nationale varslingssystem. I den forbindelse blev det oplyst, at de sundhedsfaglige myndigheder har anbefalet, at der ikke er automatik i koblingen mellem vurdering af risikoniveau og tiltag. 
Det forekommer derfor ganske besynderligt, at regeringen i forhold til rejsevejledninger har valgt at holde fast i en fuldstændig automatisk model, hvor der lukkes ned i det øjeblik, at antallet af smittede overstiger et bestemt niveau. Særligt når niveauet er sat meget lavt og er uafhængig af det samtidige smittetryk i Danmark. 
De danske sundhedsmyndigheder ses ikke at have offentliggjort endsige foretaget en sundhedsfaglig vurdering af den nuværende model for rejsevejledninger, herunder en vurdering af om restriktioner indføres for tidligt eller opretholdes i længere tid end nødvendigt. 
Sundhedsstyrelsen har dog i publikationen »COVID-19 Risikovurdering, strategi og tiltag ved epidemi i Danmark« dateret den 10. marts 2020 angivet, at øget rejseaktivitet i forbindelse med ferier kan være en blandt flere faktorer, der kan have indflydelse på udviklingen under de indledende faser af pandemien, hvorimod rejseaktivitet ikke angives som en potentiel faktor under faser med udbredt og vedvarende smittespredning. Dette kunne indikere, at de nuværende rejserestriktioner opretholdes i en situation, hvor det ikke er nødvendigt.
Verdenssundhedsorganisationen WHO har den 30. juli 2020 i en anbefaling med titlen ”Public health considerations while resuming international travel” bl.a. udtalt, at rejser mellem lande/regioner, hvor intensiteten af smitte med COVID-19 virus er sammenlignelig, ikke udgør nogen væsentlig risiko for potentiel påvirkning af den epidemiologiske situation. 
Dette er på linje med en udmelding fra European Center for Disease Prevention and Control (ECDC). ECDC anfører også i sin Rapid Risk Assesment (11. udgave) dateret 10. august 2020, at den tilgængelige dokumentation ikke understøtter rejserestriktioner i form af grænselukninger, og at det er vigtigt at beslutningstagere forstår, at en virus med en global distribution ikke kan kontrolleres med grænselukninger. 
Efter spørgerens opfattelse bør rejserestriktioner inkorporeres som mulige tiltag i det nationale varslingssystem, så de etablerede overvågnings- og beslutningsfora vil kunne håndtere spørgsmålet om eventuel indførelse af rejserestriktioner i sammenhæng med øvrige mulige tiltag. Det bemærkes endvidere, at en inkorporering af rejserestriktioner som mulige tiltag i det nationale varslingssystem ikke vil være til hinder for, at Udenrigsministeriet selvstændigt fraråder rejser til lande/regioner, hvis ministeriets vurdering af sikkerhedsniveauet begrunder dette. ', null, null, null, null, null, null, null, null, null, null);
