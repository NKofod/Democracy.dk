INSERT INTO ODA.dbo.Dokument (id, typeid, kategoriid, statusid, offentlighedskode, titel, dato, modtagelsesdato, frigivelsesdato, paragraf, paragrafnummer, spørgsmålsordlyd, spørgsmålstitel, spørgsmålsid, procedurenummer, grundnotatstatus, dagsordenudgavenummer, opdateringsdato) VALUES (305890, 17, 22, 1, N'O', N'L 164 - svar på spm. 35 om teknisk bistand til udarbejdelse af ændringsforslag, fra erhvervs- og vækstministeren', N'2015-05-18 00:00:00.000', N'2015-05-18 00:00:00.000', N'2015-05-18 13:34:49.000', N'', N'', N'Ministeren bedes levere teknisk bistand til udarbejdelse af ændringsforslag der medfører følgende:

•	Der stilles krav om enslydende etiske krav om samfundsansvar i alle udbud og kontrakter, omfattet af loven. Kravene skal stilles konkret på vareproduktionen eller leveringen, således at der i forbindelse produktionen/ leveringen ikke må være sket brud på grundlæggende arbejdstagerrettigheder (ILO-konventionens bestemmelser), menneskerettigheder, internationale miljøstandarder og international folkeret. 
•	De frivillige udelukkelsesgrunde i lovforslagets § 137, stk. 1, nr. 1, gøres obligatoriske.
•	Det sikres, at de enslydende etiske krav indskrives i alle kontrakter og udbud, når disse skal fornyes i medfør af loven.
•	Det indskrives i loven, at en leverandør indestår for sine underleverandører – dvs. er ansvarlig i forhold til ordregiveren for, at de enslydende etiske krav overholdes af den samlede leverandørkæde. 
•	Ordregiveren skal have hjemmel til at lave stikprøvekontroller hos sin leverandør i forhold til overholdelsen af de enslydende etiske krav.
•	Der skal indsættes en sanktionsbestemmelse i lovforslaget hvorefter det sikres, at en ordregiver kan hæve kontrakten uden at skulle svare erstatning, såfremt det kan påvises, at de enslydende etiske krav er overtrådt.  
•	Ordregivere skal – på samme måde som man gør det i Sverige – have mulighed for at udelukke virksomheder, hvis de pågældende virksomheder er baseret i skattely eller på anden måde unddrager sig den generelle beskatning af selskabets fortjeneste.
', N'Spm. om teknisk bistand til udarbejdelse af ændringsforslag, til erhvervs- og vækstministeren', 303945, N'', N'', null, N'2015-05-18 13:35:19.353');
