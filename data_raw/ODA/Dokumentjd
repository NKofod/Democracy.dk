INSERT INTO ODA.dbo.Dokument (id, typeid, kategoriid, statusid, offentlighedskode, titel, dato, modtagelsesdato, frigivelsesdato, paragraf, paragrafnummer, spørgsmålsordlyd, spørgsmålstitel, spørgsmålsid, procedurenummer, grundnotatstatus, dagsordenudgavenummer, opdateringsdato) VALUES (225576, 17, 22, 1, N'O', N'L 79 - svar på spm. 1, fra undervisningsministeren', N'2009-12-07 00:00:00.000', N'2009-12-07 00:00:00.000', N'2009-12-08 11:31:11.000', N'', N'', N'Ministeren bedes kommentere henvendelsen af 27/11-09 fra Efterskoleforeningen, jf. L 79 - bilag 2, herunder især følgende afsnit:  
"Efterskolerne opfordrer til at ændre udmøntningsbestemmelsen i lovforslaget og i den eksisterende lovgivning om tillægstakster: Elever omfattet af tillægstakster er sårbare og har større risiko for frafald. Netop derfor er det uheldigt, at lovforslaget viderefører en unødig bureaukratisk bestemmelse om, at tillægstaksten kun udløses, hvis en elev har gennemført mindst 12 uger af et kursus. 
Lovforslaget indebærer, at skolens foranstaltninger – mentorstøtte, etablering af dansk som andet sprog, nedslag i egenbetalingen mv. – ikke udløser tillægstakst, hvis eleven afbryder opholdet efter fx 10 eller 11 ugers forløb. Det er således skolen – og skolen alene – der afholder udgifterne til etablerede foranstaltninger i forhold til elever, der afbryder opholdet. Derudover skal skolerne lave særskilte opgørelser for elever, der har gennemført mindre end 12 uger af et kursus og elever med 12 uger eller mere. 
Denne problemstilling blev allerede rejst af Frie Kostskolers Fællesråd i forbindelse med høringssvar på L 50 i Folketingssamling 2008/09. Ministeren tilkendegav i sit svar overfor Folketinget følgende: ”Ministeriet vil i starten af det nye år foretage en samlet vurdering af behovet for ændringer af regler for tilskud til særligt prioriterede elevgrupper på de frie kostskolers område. Efterskoleforeningen, Højskolernes Forening i Danmark og Foreningen af Husholdnings- og Håndarbejdsskoler vil løbende blive inddraget i dette arbejde, og ministeriet vil sammen med skoleforeningerne arbejde på at finde hensigtsmæssige ændringer.”
Efterskoleforeningen har desværre endnu ikke været inddraget i et sådant samarbejde. Efterskoleforeningen skal derfor genfremsætte sit forslag om, at samtlige tillægstakster i § 23a udløses, når en elev deltager i et kursus af mindst 12 ugers varighed, selvom eleven falder fra inden for de første 12 uger. Taksten skal naturligvis kun udløses for de uger, eleven faktisk deltager i kurset, således at hvis en elev falder fra efter fx 7 uger udløser det 7/40 af en årselev mod nu ingenting. 
Dermed vil der for skolerne være en større tilskyndelse til at påtage sig de ekstra opgaver, tillægstaksterne tilskynder til."', N'Spm. om kommentar til henvendelsen af 27/11-09 fra Efterskoleforeningen, til undervisningsministeren', 225575, N'', N'', null, N'2015-12-05 22:45:05.987');
