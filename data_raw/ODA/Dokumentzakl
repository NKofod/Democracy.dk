INSERT INTO ODA.dbo.Dokument (id, typeid, kategoriid, statusid, offentlighedskode, titel, dato, modtagelsesdato, frigivelsesdato, paragraf, paragrafnummer, spørgsmålsordlyd, spørgsmålstitel, spørgsmålsid, procedurenummer, grundnotatstatus, dagsordenudgavenummer, opdateringsdato) VALUES (880379, 17, 22, 1, N'O', N'L 104 - svar på spm. 5 om ”anden dokumentation” for tilknytning til arbejdsmarkedet i 42 år, fra beskæftigelsesministeren', N'2020-12-10 00:00:00.000', N'2020-12-10 00:00:00.000', N'2020-12-10 16:12:47.000', N'', N'', N'Som regeringen selv bemærker, vil det i mange tilfælde være vanskeligt for den enkelte borger at indhente dokumentation for, at tilknytningen til arbejdsmarkedet er opfyldt mere end 42 år tilbage i tid – især for de ældste årgange, hvor anciennitetskravet opgøres fra 1971. Det besværliggøres yderligere, hvis en del af den opsparede anciennitet ikke kan indhentes fra offentlige systemer og registre, som f.eks. ATP eller en a-kasse, hvilket vil være gældende for størstedelen af ansøgere, som skal dokumentere arbejdstilknytning i Grønland. Af samme årsag foreslår regeringen, at i tilfælde, hvor der ikke foreligger registerdata at, ”anden dokumentation” kan anvendes, f.eks. lønsedler, udbetalingsspecifikationer eller indbetalinger til pensionsordninger. På den baggrund bedes ministeren besvare følgende spørgsmål:
• Kan ministeren uddybe hvad ”anden dokumentation” også kan være, vil det f.eks. være tilstrækkeligt med en bekræftelse af en tidligere arbejdsgiver, med en angivelse af perioden borgeren har arbejdet, samt angivelse af hvor mange timer ugentligt borgeren har arbejdet?
• I forlængelse heraf, hvad vil, i et tilfælde hvor der ikke findes tilgængelige registeroplysninger, eksempelvis for en selvstændig fisker eller fanger som har arbejdet tilbage i 70’erne, være fyldestgørende dokumentation i forhold til arbejdstilknytningen?
• I tilfælde, hvor der ikke findes tilgængelig registerdata, og den tidligere arbejdsgiver til en lønmodtager ikke længere eksisterer, hvad vil så være fyldestgørende som dokumentation? ', N'Spm. om ”anden dokumentation” for tilknytning til arbejdsmarkedet i 42 år, til beskæftigelsesministeren', 878492, N'', N'', null, N'2020-12-22 10:48:32.200');
