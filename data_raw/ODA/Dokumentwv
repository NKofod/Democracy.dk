INSERT INTO ODA.dbo.Dokument (id, typeid, kategoriid, statusid, offentlighedskode, titel, dato, modtagelsesdato, frigivelsesdato, paragraf, paragrafnummer, spørgsmålsordlyd, spørgsmålstitel, spørgsmålsid, procedurenummer, grundnotatstatus, dagsordenudgavenummer, opdateringsdato) VALUES (526123, 17, 22, 3, N'O', N'UDU alm. del - svar på spm. 314, fra undervisningsministeren', N'2008-08-21 00:00:00.000', N'2008-08-21 00:00:00.000', N'2008-08-25 13:39:12.107', N'', N'', N'Finder ministeren det i overensstemmelse med gældende regler, når skoleledere tvinges til i konkrete sager at prioritere mellem på den ene side fagligt og sagligt begrundede specialpædagogiske behov for bistand til enkeltelever og på den anden side skolens øvrige drift?  

Spørgeren henviser til, at nogle kommuner ønsker at begrænse udgifterne til specialundervisning gennem forskellige økonomiske og ikke faglige styringsmodeller. Bl.a. har nogle kommuner tildelt skolerne en given sum og derefter overlade det til skolens leder at foretage en prioritering af de konkrete behov indenfor den økonomiske ramme, der er meldt ud fra politisk hold. Denne ordning ville ikke skabe problemer, hvis den økonomiske ramme var stor nok. Men når den udmeldte ramme er for lille til at imødekomme det faktiske behov, er skolelederen nødsaget til at afvise sagligt velbegrundede krav om specialpædagogisk bistand alene på grund af manglende økonomiske ressourcer.

En sådan praksis må anses som ulovlig, da vejledningen udtrykkeligt siger: 
"Det væsentlige er, at den specialpædagogiske bistand skal gives på baggrund af en faglig og saglig vurdering af elevens undervisningsmæssige behov. Der kan således ikke i vurderingen henvises til manglende økonomiske ressourcer." (Kapitel 2)', N'Spm. om, det i overensstemmelse med gældende regler, når skoleledere tvinges til i konkrete sager at prioritere mellem på den ene side fagligt og sagligt begrundede specialpædagogiske behov for bistand til enkeltelever og på den anden side skolens øvrige drift, til undervisningsministeren ', 526122, N'', N'', null, N'2015-12-05 18:41:40.223');
