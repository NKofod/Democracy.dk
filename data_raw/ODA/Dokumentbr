INSERT INTO ODA.dbo.Dokument (id, typeid, kategoriid, statusid, offentlighedskode, titel, dato, modtagelsesdato, frigivelsesdato, paragraf, paragrafnummer, spørgsmålsordlyd, spørgsmålstitel, spørgsmålsid, procedurenummer, grundnotatstatus, dagsordenudgavenummer, opdateringsdato) VALUES (43431, 17, 22, 1, N'O', N'L 103 - svar på spm. 4 om teknisk bistand til udarbejdelse af ændringsforslag, fra social- og integrationsministeren', N'2013-02-19 00:00:00.000', N'2013-02-19 00:00:00.000', N'2013-02-19 16:51:13.000', N'', N'', N'Vil ministeren – som teknisk bistand – udarbejde ændringsforslag, der indeholder følgende:
- Ændring i § 5 stk. 2, hvor ordene: ’på hvert sit myndighedsområde’ skal udgå. Indholdet skal være at alle  pensionister, som konkret anmoder om at overgå til førtidspension efter lov om social pension, skal tilbydes vejledning og rådgivning, og at de pensionister, der anmoder om vejledning før de beslutter sig, skal vejledes og rådgives. 
- Der skal tilføjes en tekst om, at vejledningen udover Udbetaling Danmarks vejledning skal bestå af et tilbud om en personlig vejlednings- og rådgivningssamtale med en sagsbehandler i kommunen.  
- Sidste punktum skal ændres. Indholdet skal være, at vejledningen både skal gives på baggrund af pensionistens konkrete, aktuelle forhold, og at der i vejledningen og rådgivningen skal tages højde for hvilke konsekvenser, som mulige ændringer i pensionistens fremtidige helbredsforhold, boligforhold, samlivsforhold, økonomiske forhold m.v. kan have for det valg, som pensionisten træffer.

Følgende bemærkninger skal indgå:
Det kan have store konsekvenser for den enkelte pensionist, hvis han eller hun vælger forkert, da beslutningen er bindende og ikke kan fortrydes. Derfor er det vigtigt, at alle pensionister, der enten anmoder om eller overvejer at overgå til førtidspension efter lov om social pension, bliver tilbudt en grundig og fyldestgørende rådgivning og vejledning. Tilbuddet om rådgivning og vejledning skal udover den skriftlige vejledning fra Udbetaling Danmark indeholde en personlig samtale med kommunens sagsbehandler, hvor pensionisten og sagsbehandleren kan drøfte fordele og ulemper ved at overgå til den ny pension. Da pensionistens fremtidige forhold som f.eks. ændrede boligforhold, helbredsforhold og samlivsforhold kan have stor betydning for, hvilken pension der er mest fordelagtig for pensionisten, skal fremtidige forhold inddrages i rådgivningen og vejledningen.    
', N'Spm. om teknisk bistand til udarbejdelse af ændringsforslag, til social- og integrationsministeren', 43430, N'', N'', null, N'2015-12-06 06:29:30.517');
