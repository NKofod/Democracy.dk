INSERT INTO ODA.dbo.Dokument (id, typeid, kategoriid, statusid, offentlighedskode, titel, dato, modtagelsesdato, frigivelsesdato, paragraf, paragrafnummer, spørgsmålsordlyd, spørgsmålstitel, spørgsmålsid, procedurenummer, grundnotatstatus, dagsordenudgavenummer, opdateringsdato) VALUES (605221, 17, 22, 1, N'O', N'BEU alm. del - svar på spm. 336 om ministerens kommentar til fortrolig henvendelse af 10/2-2016, fra beskæftigelsesministeren', N'2016-05-04 00:00:00.000', N'2016-05-04 00:00:00.000', N'2016-05-04 16:35:00.000', N'', N'', N'Ministeren bedes kommentere fortrolig henvendelse af 10. februar 2016, jf. BEU alm. del - bilag 163.
Ministeren bedes herunder redegøre for følgende:
- Hvor oplyses det for borgeren, at denne skal søge supplerende dagpenge for at være berettiget til dagpenge?
- Ifølge Ankestyrelsens principafgørelse 63-12 har løsarbejdere, der ikke har søgt supplerende dagpenge, fraskrevet sig retten til sygedagpenge. I nærværende sag har Ankestyrelsen nu stadfæstet, at løsarbejdere heller ikke har ret til dagpenge. Hvor i loven findes hjemmel for dette? Der henvises i denne forbindelse til beskæftigelsesministerens vedlagte svar på spørgsmål S 1059, folketingsåret 2013-14.
- Om ministeren synes, at disse vilkår er rimelige, og om ministeren er af den overbevisning, at de beskrevne forhold vil øge incitamentet til at tage korterevarende jobs?', N'Spm. om ministerens kommentar til fortrolig henvendelse af 10/2-2016, til beskæftigelsesministeren', 601063, N'', N'', null, N'2016-08-29 15:38:28.613');
