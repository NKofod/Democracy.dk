INSERT INTO ODA.dbo.Dokument (id, typeid, kategoriid, statusid, offentlighedskode, titel, dato, modtagelsesdato, frigivelsesdato, paragraf, paragrafnummer, spørgsmålsordlyd, spørgsmålstitel, spørgsmålsid, procedurenummer, grundnotatstatus, dagsordenudgavenummer, opdateringsdato) VALUES (25876, 17, 30, 1, N'O', N'TRU alm. del - foreløbigt svar på spm. 728 om, hvordan man skal forholde sig, hvis man skulle være i besiddelse af et ulovligt produkt, fra transportministeren', N'2014-06-19 00:00:00.000', N'2014-06-19 00:00:00.000', N'2014-06-19 13:46:39.000', N'', N'', N'En meddelelse om tilbagekaldelse indeholder normalt følgende oplysninger om produktet:
- Et detaljeret foto af produktet 
- Leverandørens eller producentens kontaktinformation
- Produktets type- eller handelsbetegnelse 
- Beskrivelse af hvori problemet består
- Hvordan man skal forholde sig, hvis man skulle være i besiddelse af et ulovligt produkt.

Disse oplysninger mangler, og samtidig  har Trafikstyrelsen helt undladt at opliste og vise fotos af flere af de ulovlige produkter i de udsendte meddelelser til offentligheden og synsvirksomhederne. 
Det drejer sig bl.a. om en ulovlig seleforlænger, men der er flere produkter, som ikke er nævnt, og som sælges til anvendelse i såvel private personbiler og i busser. Som navnet antyder bruges seleforlængeren til at forlænge sikkerhedsselen, så det f.eks. er muligt at fastspænde flere barnestole ved siden af hinanden, eller på steder hvor det er svært at komme til.  Seleforlængeren udmærker sig ved, at være konstrueret, så den passer til alle disse forskellige fabrikater selelåse, men den er ikke blevet testet og godkendt til en eneste af dem, og kan derfor gå op under en kollision, hvilket selvsagt kan få katastrofale følger. Vil ministeren sørge for, at der nu gennemføres en rigtig og effektiv tilbagekaldelse, hvor de ovennævnte oplysninger fremgår, så det fremgår krystalklart, hvilke produkter, der tilbagekaldes, og hvem der har markedsført dem?', N'MFU spm. om, hvordan man skal forholde sig, hvis man skulle være i besiddelse af et ulovligt produkt, til transportministeren', 25674, N'', N'', null, N'2014-09-17 03:25:03.813');
