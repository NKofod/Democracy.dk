INSERT INTO ODA.dbo.Dokument (id, typeid, kategoriid, statusid, offentlighedskode, titel, dato, modtagelsesdato, frigivelsesdato, paragraf, paragrafnummer, spørgsmålsordlyd, spørgsmålstitel, spørgsmålsid, procedurenummer, grundnotatstatus, dagsordenudgavenummer, opdateringsdato) VALUES (414833, 17, 22, 3, N'O', N'REU alm. del - svar på spørgsmål 100, fra justitisministeren', N'2005-01-11 00:00:00.000', N'2005-01-12 00:00:00.000', N'2005-01-12 22:29:27.000', N'', N'', N'Hvad vil ministeren gøre for at sikre overholdelsen af persondatadirektivet efter dets ånd og bogstav således, at der i forbindelse med ændringer i Schengenlandenes it-system SIS'' nye version 2 sikres proportionalitet imellem hensynet til sikringen af individets privatliv og øget internationalt it-samarbejde mellem offentlige myndigheder?
  
Der henvises til de indtrængende advarsler, Den Fælles Tilsynsmyndighed, der består af repræsentanter for deltagerlandenes egne Datatilsyn, i sin sjette rapport retter mod de tendenser i retning af mange flere brugere, flere registreringer, nye funktioner, flere internationale videregivelser og medinddragelse af nye formål, det kommende system risikerer at blive brugt til, således at vi får et reelt Big-Brother-system i hele EU, hvor alle, der blot er rejst en mindre mistanke imod, umiddelbart registrereres i et internationalt system.

Rapporten henleder opmærksomheden på store biometribaser og koblingen til den europæiske arrestordre og udtrykker i sjældent tydelige vendinger sin meget store betænkeligheder ved den udvikling, der tilsyneladende er på vej ud i et betænkeligt selvsving.

', N'Spm. om Schengenlandenes it-system, til justitministeren', 414832, N'', N'', null, N'2015-12-03 21:56:47.103');
