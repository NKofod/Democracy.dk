INSERT INTO ODA.dbo.Sag (id, typeid, kategoriid, statusid, titel, titelkort, offentlighedskode, nummer, nummerprefix, nummernumerisk, nummerpostfix, resume, afstemningskonklusion, periodeid, afgørelsesresultatkode, baggrundsmateriale, opdateringsdato, statsbudgetsag, begrundelse, paragrafnummer, paragraf, afgørelsesdato, afgørelse, rådsmødedato, lovnummer, lovnummerdato, retsinformationsurl, fremsatundersagid, deltundersagid) VALUES (37307, 10, 11, 20, N'Vil ministeren oplyse, om bevilling af hjemmehjælp efter servicelovens § 71 samt håndsrækninger som merudgiftsydelse efter servicelovens § 84 er udelukket, alene fordi man har en rask ægtefælle eller samlever?', N'Om bevilling af hjemmehjælp efter servicelovens § 71.', N'O', N'S 1180', N'S', N'1180', N'', N'', null, 22, null, null, N'2017-12-07 17:52:56.760', 0, N'Både ved bevilling af hjemmehjælp og håndsrækninger som merudgiftsydelse har den praksis udviklet sig, at hjælp er udelukket, hvis man har en rask ægtefælle eller samlever.
Spørgeren har forsøgt at orientere sig i de gældende regelsæt, men har intet fundet om denne problemstilling i forbindelse med almindelig hjemmehjælp. Derimod fremgår det af Socialministeriets vejledning af 23. oktober 2002 om merudgifter, "at i forbindelse med vurderingen af om den pågældende har nødvendige merudgifter til almindelige daglige håndsrækninger skal kommunen vurdere den pågældendes samlede situation. Der skal blandt andet tages hensyn til den pågældendes netværk, og det forudsættes, at eventuelle øvrige medlemmer af husstanden deltager i opgaveudførelsen i hjemmet &".
Ud fra almindelig sproglig logik indebærer denne formulering imidlertid ikke, at øvrige medlemmer af husstanden skal udføre samtlige opgaver i hjemmet. Ud fra princippet om sammenligning med andre mennesker på samme alder og i samme livssituation forekommer et mere rimeligt udgangspunkt at være, at man deler opgaverne i hjemmet, og den person, som ikke har en funktionsnedsættelse, vil i så fald varetage cirka halvdelen af opgaverne, mens personen med en funktionsnedsættelse bør have hjælp, så vedkommende alene eller med den bevilgede hjælp kan udføre nogenlunde den anden halvdel.
Det er særdeles belastende for et parforhold, hvis den ene part føler, at han eller hun skal påtage sig alle opgaver i hjemmet, eller hvis den anden part føler, at han eller hun er en belastning for sin ægtefælle/samlever. Denne uligevægt i et parforhold fører i praksis til brudte parforhold og skilsmisser.
Det forekommer derfor urimeligt, at praksis, endog med ankesystemets godkendelse, har udviklet sig til, at man aldrig kan få hjemmehjælp eller håndsrækninger som merudgiftsydelse, hvis man har en rask ægtefælle eller samlever.', null, null, null, null, null, null, null, null, null, null);
