INSERT INTO ODA.dbo.Dokument (id, typeid, kategoriid, statusid, offentlighedskode, titel, dato, modtagelsesdato, frigivelsesdato, paragraf, paragrafnummer, spørgsmålsordlyd, spørgsmålstitel, spørgsmålsid, procedurenummer, grundnotatstatus, dagsordenudgavenummer, opdateringsdato) VALUES (531257, 17, 22, 3, N'O', N'UDU alm. del - svar på spm. 6, fra undervisningsministeren', N'2008-10-28 00:00:00.000', N'2008-10-28 00:00:00.000', N'2008-10-29 10:08:56.597', N'', N'', N'Det bedes oplyst, om der her i landet foreligger forskning, der belyser konsekvenserne af, at skolebørn, der tilhører etniske/kulturelle mindretal, i skoleti-den har adgang til at tale med hinanden på et såkaldt ”hemmeligt sprog”, dvs. et fremmedsprog, som lærere og danske skolekammerater i overvejende grad ikke forstår. 

Hvis der foreligger sådan forskning, bedes det kort refereret, hvilken betydning forskerne tillægger brugen af ”hemmeligt sprog”, herunder for integrationen, for tilegnelse af kundska-ber, i forhold til mobning og i forhold til risikoen for, at der i skoletiden drøftes eller aftales illegitime forhold?

Hvis der ikke foreligger dansk forskning om emnet, bedes det om muligt oplyst, om der i USA eller UK foreligger forskning om emnet.
