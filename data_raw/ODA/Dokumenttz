INSERT INTO ODA.dbo.Dokument (id, typeid, kategoriid, statusid, offentlighedskode, titel, dato, modtagelsesdato, frigivelsesdato, paragraf, paragrafnummer, spørgsmålsordlyd, spørgsmålstitel, spørgsmålsid, procedurenummer, grundnotatstatus, dagsordenudgavenummer, opdateringsdato) VALUES (470448, 17, 22, 3, N'O', N'FIU - Svar på § 7 spm. 2 L 1 FFL 2007, fra finansministeren', N'2006-11-30 00:00:00.000', null, N'2006-11-30 13:56:21.627', N'Finansministeriet', N'7', N'Ad FFL 2007 generelt

I Finansudvalgets beretning af 27. oktober 2005 hedder det under pkt. 5.2. bl.a., at
•	antallet af tekstanmærkninger bør begrænses mest muligt, 
•	ændring af eksisterende lovgivning kun bør finde sted, såfremt
ændringen er tidsbegrænset, eller at ændring af lovgivningen ikke kan afventes, 
•	at varige eller generelle lovændringer ikke bør gennemføres ved tekstanmærkning og
•	at tekstanmærkninger af materiel art ikke bør opføres uændrede på finansloven over en årrække, men i stedet bør indarbejdes i gældende materiel lov.
I Finansministeriets Budgetvejledning 2006 hedder det under pkt. 1.5. bl.a., at ændring af eksisterende lovgivning ved tekstanmærkning kun bør finde sted, såfremt ændringen er tidsbegrænset eller ændring af lovgivningen ikke kan afventes. 
På denne baggrund ønskes ministerens besvarelse af følgende spørgsmål:

Af ministerens svar på Finansudvalgets spørgsmål 2 af 7. september 2006 vedrørende FFL 2007 fremgår, at et meget betydeligt antal tekstanmærkninger har været opført uændret på finansloven i en meget lang periode. Adskillige tekstanmærkninger har således været på finansloven i over 40 år. På denne baggrund ønskes oplyst, hvorledes dette harmonerer med det i Finansudvalgets beretning og Budgetvejledningen anførte? Hvilke initiativer har ministeren taget og agter ministeren at tage med henblik på at opfylde ministerens i beretningen refererede tilsagn om, tekstanmærkninger vil blive indarbejdet i gældende lovgivning, når lejlighed byder sig? Hvilke tekstanmærkninger, der var opført på FL 2006, er blevet indarbejdet i gældende lovgivning og er af denne grund udgået på FFL 2007?













', N'Spm. om tekstanmærkninger, til finansministeren', 470447, N'', N'', null, N'2015-12-05 14:14:07.507');
