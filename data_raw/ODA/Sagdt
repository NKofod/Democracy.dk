INSERT INTO ODA.dbo.Sag (id, typeid, kategoriid, statusid, titel, titelkort, offentlighedskode, nummer, nummerprefix, nummernumerisk, nummerpostfix, resume, afstemningskonklusion, periodeid, afgørelsesresultatkode, baggrundsmateriale, opdateringsdato, statsbudgetsag, begrundelse, paragrafnummer, paragraf, afgørelsesdato, afgørelse, rådsmødedato, lovnummer, lovnummerdato, retsinformationsurl, fremsatundersagid, deltundersagid) VALUES (45501, 10, 11, 20, N'Vil ministeren oplyse, om der i reglerne for Schengen-samarbejdet vedrørende kontrol af personers identitet i forbindelse med ind- og udrejse findes undtagelser, der fritager personer iført religiøs påklædning for denne kontrol, samt vurdere, om det kan tænkes, at nogle grænsemyndigheder i misforstået politisk korrekthed undlader at gennemføre den fornødne identitetskontrol af personer iført religiøs påklædning?', N'Om kontrol af personers identitet ved ind- og udrejse', N'O', N'S 749', N'S', N'749', N'', N'', null, 23, null, null, N'2017-12-05 22:53:30.540', 0, N'Ved personkontrollen i danske lufthavne foretages ikke kontrol af overensstemmelse mellem en tilsløret passagers identitet og dennes pas eller anden rejsehjemmel. Tilsvarende gælder ved anden paskontrol.

Der er således tale om en diskrimination mod ikke tilslørede personer uanset, at de aktuelle trusler mod flysikkerheden og den ulovlige indvandring i særlig grad hidrører fra personer, hvis kultur indebærer tilsløring af kvinder. Det er således uden videre muligt for tilslørede personer at medbringe sprængfarlige væsker i forbudte mængder, som ikke kan afsløres i metaldetektoren, skjult under en hijab eller burka.

Medens andre flypassagerer nu tvinges til at aflægge jakke, bælter og ofte tillige sko inden passage af metaldetektoren, er der således særrettigheder for personer, der hævder, at overjordiske magter har påbudt dem at være tilsløret.

Dagbladet BT har således uden vanskeligheder sendt en dansk journalist, formummet i burka med fly fra Kastrup lufthavn til Stansted lufthavn, London, jf. nævnte dagblad den 23. oktober 2006.

Spørgeren ønsker i forlængelse heraf at få belyst baggrunden for denne forskelsbehandling.', null, null, null, null, null, null, null, null, null, null);
