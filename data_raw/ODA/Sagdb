INSERT INTO ODA.dbo.Sag (id, typeid, kategoriid, statusid, titel, titelkort, offentlighedskode, nummer, nummerprefix, nummernumerisk, nummerpostfix, resume, afstemningskonklusion, periodeid, afgørelsesresultatkode, baggrundsmateriale, opdateringsdato, statsbudgetsag, begrundelse, paragrafnummer, paragraf, afgørelsesdato, afgørelse, rådsmødedato, lovnummer, lovnummerdato, retsinformationsurl, fremsatundersagid, deltundersagid) VALUES (34931, 10, 11, 20, N'Vil ministeren oplyse, om amter og kommuner gennem aftaler efter retssikkerhedslovens § 42, stk. 3, vil kunne begrænse sagsbehandlingstiden for ansøgninger om reparation af særlige indretninger i handicapbiler, og vil ministeren i givet fald henlede amternes og kommunernes opmærksom herpå?', N'Om ansøgninger om reparation af særlige indretninger i handicapbiler.', N'O', N'S 3063', N'S', N'3063', N'', N'', null, 21, null, null, N'2017-12-07 18:48:55.960', 0, N'I sin besvarelse af spørgsmål nr. S 1099 har ministeren redegjort for den forventede sagsbehandlings- tid i Århus Amt, når personer med en handicapbil og særlige indretninger heri ansøger om reparation af disse.
Selv om den af ministeren beskrevne sagsbehandlingstid er kortere end sagsbehandlingstiden ved mange andre sager, er selv en uge faktisk en lang periode, hvis man er ude af stand til at bruge sin ofte eneste transportmulighed, fordi man herved vil være afskåret fra enhver aktivitet uden for hjemmet, f.eks. behandling, arbejde, uddannelse eller fritidsaktiviteter.
Ud over amtets sagsbehandlingstid er der jo også en forudgående sagsbehandlingstid som følge af, at alle ansøgninger vedrørende handicapbiler skal indgives til kommunen, som forbereder sagen, før den videresendes til amtets afgørelse.
Reglerne om dobbelt sagsbehandling i forbindelse med handicapbiler kan måske være relevante i store indviklede og kostbare sager, der handler om bevilling af selve bilen; men en ansøgning om reparation af en særlig indretning i bilen er jo en ganske simpel sag af beskeden økonomisk rækkevidde, hvilket også illustreres af, at det beløb, der blev afsat til formålet i forbindelse med finansloven for 2005, kun udgør 10 mio. kr. på landsplan for et helt år.
Det fremgår imidlertid af retssikkerhedslovens § 42, stk. 3, at kommuner og amtskommuner kan »indgå aftaler, hvorefter forpligtelser, der efter den sociale lovgivning påhviler den ene part, kan overlades til den anden«.
Det ønskes oplyst, om denne bestemmelse vil kunne anvendes i disse oftest ganske simple sager, således at den dobbelte sagsbehandling og formentlig mindst halvdelen af sagsbehandlingstiden allerede herved vil kunne spares, samt i givet fald om ministeren vil henlede amternes og kommunernes opmærksomhed herpå.', null, null, null, null, null, null, null, null, null, null);
