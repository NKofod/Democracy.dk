INSERT INTO ODA.dbo.Sag (id, typeid, kategoriid, statusid, titel, titelkort, offentlighedskode, nummer, nummerprefix, nummernumerisk, nummerpostfix, resume, afstemningskonklusion, periodeid, afgørelsesresultatkode, baggrundsmateriale, opdateringsdato, statsbudgetsag, begrundelse, paragrafnummer, paragraf, afgørelsesdato, afgørelse, rådsmødedato, lovnummer, lovnummerdato, retsinformationsurl, fremsatundersagid, deltundersagid) VALUES (34823, 10, 11, 20, N'Kan ministeren bekræfte oplysningerne i Erhvervsbladets artikel den 10. august 2005: »Kommuner forgylder jordejere ved ekspropriation til boligbyggeri«, hvorefter kommunerne i stedet for at opkøbe arealer fra private grundejere, foretager ekspropriation af arealerne (til markedsværdi), således at avancen ved afståelsen bliver skattefri efter reglerne i ejendomsavancebeskatningslovens § 11, samt at denne ekspropriation foregår med jordejerens tilslutning?', N'Om ekspropriation til boligbyggeri.', N'O', N'S 2970', N'S', N'2970', N'', N'', null, 21, null, null, N'2017-12-07 18:48:39.287', 0, N'Ifølge Erhvervsbladet den 10. august 2005 er det meget normalt, at kommunerne ved opkøb af jordarealer til udstykning til boligformål aftaler med sælgeren, at opkøbet sker som eller kunne ske som ekspropriation, idet der derved er skattefrihed for den store gevinst ejeren af de overdragne arealer opnår. Dette sker, fordi afståelse ved ekspropriation er skattefrit, jf. bestemmelserne i § 11 i ejendomsavancebeskatningsloven. Da arealerne overtages til aktuel handelsværdi, er det helt urimeligt, at grundejerne fritages for avancebeskatning. Dette er så meget mere urimeligt, som der nu ikke opkræves frigørelsesafgift, der jo blev ophævet ved lov nr. 458 af 9. juni 2004 på regeringens foranledning. Da sælgerne jo opnår markedspris for den solgte jord, er det urimeligt, at de pågældende forgyldes med skattefrihed. De konstaterede avancer er jo udtryk for værdistigninger, der først og fremmest er skabt som følge af samfundsudviklingen, herunder den lokale byudvikling. Det er også meget svært at se retfærdigheden i, at en landmand, der sælger sin jord til en anden landmand med henblik på fortsat landbrugsdrift skal betale skat af ejendomsavancen, medens det er skattefrit, hvis der sker afhændelse i form af ekspropriation til kommunen med henblik på kommende boligbyggeri. Normalt vil avancen ved overdragelse til kommune være betydeligt større end ved salget til landmand med henblik på fortsat landbrugsdrift.', null, null, null, null, null, null, null, null, null, null);
