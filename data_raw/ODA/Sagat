INSERT INTO ODA.dbo.Sag (id, typeid, kategoriid, statusid, titel, titelkort, offentlighedskode, nummer, nummerprefix, nummernumerisk, nummerpostfix, resume, afstemningskonklusion, periodeid, afgørelsesresultatkode, baggrundsmateriale, opdateringsdato, statsbudgetsag, begrundelse, paragrafnummer, paragraf, afgørelsesdato, afgørelse, rådsmødedato, lovnummer, lovnummerdato, retsinformationsurl, fremsatundersagid, deltundersagid) VALUES (7641, 3, 13, 11, N'Forslag til lov om ændring af lov om værdipapirhandel m.v. og lov om finansiel virksomhed.  (Gennemførelse af markedsmisbrugs- og prospektdirektivet).', N'Om værdipapirhandel m.v. og finansiel virksomhed.', N'O', N'L 13', N'L', N'13', N'', N'Loven implementer en række nye EF-direktiver om bekæmpelse af markedsmisbrug ved handel med værdipapirer og udarbejdelse af prospekter, når værdipapirer udbydes til offentligheden. De væsentligste ændringer som følge af implementeringen er,
- at tidspunktet for, hvornår reglerne om misbrug af intern viden og kursmanipulation er gældende, fremrykkes, således at de gælder allerede fra det tidspunkt, hvor der ansøgt om optagelse på en børs eller et reguleret marked,
- at der bliver fastsat detaljerede regler for, hvilke oplysninger, som er relevante for kursdannelsen, udstederen skal offentliggøre,
- at ledende medarbejderes handel med aktier i udstederselskabet skal offentliggøres, og
- at der bliver indført ens regler for prospekter for værdipapirer, der søges optaget til notering eller handel på en fondsbørs eller et lignende reguleret marked, og værdipapirer, der i øvrigt udbydes til offentligheden.

Baggrund:
	Formålet med lovforslaget var at gennemføre en række nye EF-direktiver. Det drejede sig om følgende direktiver: 
