INSERT INTO ODA.dbo.Dokument (id, typeid, kategoriid, statusid, offentlighedskode, titel, dato, modtagelsesdato, frigivelsesdato, paragraf, paragrafnummer, spørgsmålsordlyd, spørgsmålstitel, spørgsmålsid, procedurenummer, grundnotatstatus, dagsordenudgavenummer, opdateringsdato) VALUES (506920, 17, 22, 3, N'O', N'REU alm. del - svar på spm. 365, fra justitsministeren', N'2008-03-26 00:00:00.000', N'2008-03-26 00:00:00.000', N'2008-03-27 10:20:54.937', N'', N'', N'Vurderer ministeren, at det er tilfredsstillende, når et opkald til alarmcentral på nr. 112 med anmeldelse af et overfald umiddelbart efter, at dette er sket, bliver afvist med anmodning om, at anmelderen i stedet ringer til den politistation i politikredsen, hvor der er døgnåbent?

Ministeren bedes oplyse, om den service med opkald til 114, som skulle gå direkte til politiet i den politikreds, man befinder sig i, rent faktisk virker. Hvis dette er tilfældet, hvorfor har man så ikke gjort mere for at informere offentligheden om denne mulighed for at tilkalde politihjælp?

Der henvises til, at i en konkret sag fra natten mellem fredag og lørdag 25./26. januar 2008 blev en person overfaldet på Birkerød Hovedgade, og da den pågældende umiddelbart efter ringede til 112 på sin mobiltelefon, fik han af alarmcentralen (i København) at vide, at man ikke kunne modtage anmeldelsen, men at han selv skulle ringe til Nordsjællands Politi, da man ikke kunne stille videre. Efterfølgende har den pågældende fået oplyst, at han kunne have ringet til 114 og derved kommet i direkte forbindelse med politikredsens døgnbemandede politistation.
', N'Spm. om opkald til 112 og 114, til justitsministeren', 506919, N'', N'', null, N'2015-12-05 17:24:24.403');
