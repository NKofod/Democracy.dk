INSERT INTO ODA.dbo.Sag (id, typeid, kategoriid, statusid, titel, titelkort, offentlighedskode, nummer, nummerprefix, nummernumerisk, nummerpostfix, resume, afstemningskonklusion, periodeid, afgørelsesresultatkode, baggrundsmateriale, opdateringsdato, statsbudgetsag, begrundelse, paragrafnummer, paragraf, afgørelsesdato, afgørelse, rådsmødedato, lovnummer, lovnummerdato, retsinformationsurl, fremsatundersagid, deltundersagid) VALUES (97001, 8, null, 21, N'Aktstykke om overførsel af midler fra købmandsordningen til turismeindsatser ', N'Aktstykke om overførsel af midler fra købmandsordningen til turismeindsatser ', N'O', N'Aktstk. 92', N'Aktstk.', N'92', N'', N'', null, 160, N'TF', null, N'2023-12-14 19:42:55.470', 0, null, 8, N'Erhvervsministeriet', N'2023-12-14 00:00:00.000', N'Tiltrådt af et flertal i Finansudvalget den 14-12-2023 (udvalget med undtagelse af Socialistisk Folkepartis, Liberal Alliances, Det Konservative Folkepartis, og Enhedslistens medlem, der stemte imod aktstykket)
Påtegning
Socialistisk Folkepartis medlem af udvalget bemærker, at SF hilser initiativerne om styrkelse af turisme i mindre byer velkomment, men finder det bekymrende, at erhvervsministeren fordeler ubrugte midler fra en politisk aftale uden en reel involvering af aftalepartierne. 
Partierne bag aftale om støtteordning for mindre købmænd og andre energiudsatte fødevareforretninger i små byer ramt af stigende energipriser indgik en aftalen i februar 2023. I december 2023 viser det sig så, at der er ca. 41 mio. kr. tilovers, som erhvervsministeren ønsker at bruge på turismetiltag. Partierne får over weekenden til at tilslutte sig midlernes nye anvendelsesformål. 
SF finder det kritisabelt, at ministeriet sender aftalen i høring og ikke lytter til SF’s input. SF vil indstille, at man i fremtiden går tilbage til forhandlingsbordet og finder en fælles forståelse for, hvordan overskydende midler fra politiske aftaler anvendes.
SF havde indstillet dette, da ministeriet gjorde opmærksom på, at der skulle ske en omprioriteringen af midlerne, men dette ønske blev forbigået af erhvervsministeren. SF finder det problematisk, at en politisk aftale ikke respekteres, når uforbrugte midler kan omprioriteres og tildeles uden nye forhandlinger i den politiske aftalekreds.', null, null, null, null, null, null);
