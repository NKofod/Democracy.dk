INSERT INTO ODA.dbo.Dokument (id, typeid, kategoriid, statusid, offentlighedskode, titel, dato, modtagelsesdato, frigivelsesdato, paragraf, paragrafnummer, spørgsmålsordlyd, spørgsmålstitel, spørgsmålsid, procedurenummer, grundnotatstatus, dagsordenudgavenummer, opdateringsdato) VALUES (837918, 17, 22, 1, N'O', N'Aktstk. 178 - svar på spm.  8 om beskrivelse af situationen for venues er fair og rimelig eller om ministeren vil ændre bestemmelserne, fra erhvervsministeren', N'2020-05-28 00:00:00.000', N'2020-05-28 00:00:00.000', N'2020-05-28 06:52:03.000', N'Erhvervs- og Vækstministeriet', N'8', N'Kompensationsordningen dækker for arrangører, men efterlader venues så som konferencecentre, konferencehoteller, kongres- og messecentre og sta-dion/arenaer fuldstændig i stikken fsva aflysning/flytning af arrangementer som de ikke selv er arrangører af, og som aflyser/flytter under påberåbelse af force majeure.
Det fremgår eksplicit af bilaget “Håndtering af Force Majeure” at hvis/når en arrangør aflyser/flytter på grund af force majeure, hvilket det er i de perioder hvor der er forsamlingsforbud, så modtager de ikke kompensation for de udgifter de kan slippe ud af under henvisning til force majeure, hvil-ket er alle omkostninger på venue delen.
Disse omkostninger er bl.a. lokaleleje, som venue så kan modtage omkost-ningsdækning på op til 80% på, men de store messer, kongresser m.m. har store forlods udgifter til planlægning m.m. som ofte foregår mange måne-der / år forud for afholdelse, og kan løbe op i store beløb primært til løn-ninger og administration. Disse udgifter sidder venue tilbage med uden no-gen former for kompensation eller lignende.
Dette gælder så iøvrigt også for alle de venues som har fået aflysninger fra udenlandske arrangører. Her er der heller ingen dækning for de ofte store forlodsudgifter som venue måtte have afholdt.
