INSERT INTO ODA.dbo.Dokument (id, typeid, kategoriid, statusid, offentlighedskode, titel, dato, modtagelsesdato, frigivelsesdato, paragraf, paragrafnummer, spørgsmålsordlyd, spørgsmålstitel, spørgsmålsid, procedurenummer, grundnotatstatus, dagsordenudgavenummer, opdateringsdato) VALUES (1092287, 17, 22, 1, N'O', N'ERU alm. del - svar på MFU spm. 199 om, hvad ministerens holdning er til at indføre samme regler for tilsyn med bankernes rente og indlånsmarginal som det britiske FCA', N'2024-03-18 00:00:00.000', N'2024-03-18 00:00:00.000', N'2024-03-18 22:42:15.000', N'', N'', N'Hvad er ministerens holdning til at indføre samme regler for tilsyn med bankernes rente og indlånsmarginal som det britiske FCA – herunder: 
- Finanstilsynet skal hvert halve år udarbejde en rangordnet liste med de banker, der har de hhv. laveste og højeste indlånsrenter på markedet ved anfordringsindskud og indlån med forskellige bindinger på opsigelse og formål. 
- Finanstilsynet skal undersøge, om de banker, der giver de laveste indlånsrenter på markedet, aktivt har rådgivet relevante kunder om mere fordelagtige indlånsprodukter hos banken. 
- Finanstilsynet skal kræve, at bankerne redegør for indlånsrenteniveauerne på hhv. tilbud til nye kunder og indlånsrenteniveauerne for eksisterende kunder. 
- Finanstilsynet skal gennemføre et årligt pristjek af pengeinstitutternes rentesatser på hhv. udlån og indlån sammenholdt med Nationalbankens renteudvikling. Pristjekket skal bidrage til at vurdere om markedet er konkurrencedygtigt og sikrer forbrugerne rimelige rentesatser. I pristjekket bør endvidere indgå andre priser såsom gebyrer. 
- Finanstilsynet skal overvåge bankernes tempo i at ændre deres renter på indlånskonti – herunder anfordringskonti, når nationalbank-renten sættes op. 
Der henvises til pressemeddelelsen ”FCA sets out 14-point action plan on cash savings”, den 31. juli 2023 (https://www.fca.org.uk/news/press-releases/action-plan-cash-savings).', N'MFU spm. om, hvad ministerens holdning er til at indføre samme regler for tilsyn med bankernes rente og indlånsmarginal som det britiske FCA', 1089730, N'', N'', null, N'2024-03-18 22:42:26.900');
