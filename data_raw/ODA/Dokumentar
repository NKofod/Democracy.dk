INSERT INTO ODA.dbo.Dokument (id, typeid, kategoriid, statusid, offentlighedskode, titel, dato, modtagelsesdato, frigivelsesdato, paragraf, paragrafnummer, spørgsmålsordlyd, spørgsmålstitel, spørgsmålsid, procedurenummer, grundnotatstatus, dagsordenudgavenummer, opdateringsdato) VALUES (16581, 17, 22, 1, N'O', N'FIV alm. del - svar på spm. 219, fra ministeren for forskning, innovation og videregående uddannelser ', N'2012-10-10 00:00:00.000', N'2012-10-10 00:00:00.000', N'2012-10-10 14:23:47.000', N'', N'', N'Ministeren skriver i sit svar (FIV alm.del - svar på spm. 187)
"I forbindelse med indførelsen af handicaptillægget i 2004 blev det samtidig præciseret, at studerende, som modtager handicaptillæg, forventes at kunne gennemføre uddannelsen på almindelige SU-vilkår. Det betyder, at de kan opfylde kravet om at være fuldtidsstuderende som alle andre SU-modtagende studerende. 
Studerende med så omfattende handicap, at de ikke kan gennemføre uddannelsen på almindelige vilkår, er efter min opfattelse bedst tjent med at søge revalidering, hvor der i højere grad kan tages individuelle hensyn for eksempel ved forlængelse af studietiden."
Kan ministeren bekræfte, at ankestyrelsen har bestemt, at en revalideringsplan for en videregående uddannelse ikke behøver at være længere end til bachelorniveau; Og mener ministeren ikke, at unge med et fysisk handicap er bedre stillet med en videregående uddannelse, og at det derfor kan være fornuftigt - ikke mindst i lyset af regeringens politik om at færre unge skal have førtidspension - at det skal være muligt for studerende med nedsat arbejdsevne, som ikke kan leve op til kravet om at være fuldtidsstuderende, at gennemføre en videregående uddannelse til kandidatniveau?', N'MFU spm. om ministeren vil bekræfte, at ankestyrelsen har bestemt, at en revalideringsplan for en videregående uddannelse ikke behøver at være længere end til bachelorniveau, til ministeren for forskning, innovation og videregående uddannelser', 16582, N'', N'', null, N'2015-12-06 05:28:36.110');
