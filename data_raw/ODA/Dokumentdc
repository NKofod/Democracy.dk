INSERT INTO ODA.dbo.Dokument (id, typeid, kategoriid, statusid, offentlighedskode, titel, dato, modtagelsesdato, frigivelsesdato, paragraf, paragrafnummer, spørgsmålsordlyd, spørgsmålstitel, spørgsmålsid, procedurenummer, grundnotatstatus, dagsordenudgavenummer, opdateringsdato) VALUES (83044, 17, 22, 1, N'O', N'L 35 - svar på spm. 30 om bemærkningerne til lovforslaget om delautoriserede virksomheder, fra erhvervs- og vækstministeren', N'2014-03-31 00:00:00.000', N'2014-03-31 00:00:00.000', N'2014-03-31 15:10:14.000', N'', N'', N'Af bemærkningerne til lovforslaget fremgår det, at i de delautoriserede virksomheder skal den fagligt ansvarlige have de samme faglige kompetencer, som der stilles krav til i fuldt autoriserede virksomheder i forhold til det forretningsområde, som delautorisationen omfatter. 

- Hvilke kompetence skal den faglig ansvarlig have for at kunne godkendes af sikkerhedsstyrelsen som boliginstallatør, skal der udover boliginstallatøruddannelsen, være anden forudgående uddannelse for at blive godkendt af sikkerhedsstyrelsen? 

- Hvilke områder fra den nuværende elinstallatøruddannelse vil det ikke være nødvendigt at boliginstallatør bliver uddannet i, i betragtning af at uddannelsen kun skal være på 1/4 af den fulde autorisation. 

- Vil en boliginstallatør kunne opnå retten til at udarbejde elinstallationsrapport som led i huseftersynsordning ved ejer skift.
