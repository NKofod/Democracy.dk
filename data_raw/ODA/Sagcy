INSERT INTO ODA.dbo.Sag (id, typeid, kategoriid, statusid, titel, titelkort, offentlighedskode, nummer, nummerprefix, nummernumerisk, nummerpostfix, resume, afstemningskonklusion, periodeid, afgørelsesresultatkode, baggrundsmateriale, opdateringsdato, statsbudgetsag, begrundelse, paragrafnummer, paragraf, afgørelsesdato, afgørelse, rådsmødedato, lovnummer, lovnummerdato, retsinformationsurl, fremsatundersagid, deltundersagid) VALUES (33007, 10, 11, 20, N'Vil ministeren sikre, at behandling med vederlagsfri fysioterapi ikke bortfalder, alene fordi den viser sig at være virkningsfuld?', N'Om overenskomst med fysioterapeuter.', N'O', N'S 1462', N'S', N'1462', N'', N'', null, 21, null, null, N'2017-12-07 18:37:56.593', 0, N'I den nye overenskomst mellem Sygesikringens Forhandlingsudvalg og Danske Fysioterapeuter er målgruppen for vederlagsfri fysioterapi fastlagt ud fra en definition på »et svært fysisk handicap«, som siger, at »en patient, som kan klare sig selv indendørs i døgnets 24 timer uden hjælp eller hjælpemidler til den daglige livsførelse, har ikke et svært handicap«.
 
Dette indebærer, både sprogligt og efter 4 måneders praksis med den nye overenskomst, at man har ret til vederlagsfri fysioterapi, hvis man i sit eget hjem har brug for hjælpemidler eller hjælp, f.eks. fra en ægtefælle. Men hvis den vederlagsfri fysioterapi viser sig at være så virkningsfuld, at man kan klare sig uden hjælp i eget hjem, bortfalder retten til vederlagsfri fysioterapi.
 
Uden behandling vil en række diagnosegrupper imidlertid igen miste færdigheder, men først når tabet af færdigheder er så alvorligt, at der igen er behov for hjælp i dagligdagen, generhverves retten til vederlagsfri fysioterapi.
 
Efter spørgerens opfattelse er den anvendte formulering derfor særdeles uheldig, og retten til vederlagsfri fysioterapi burde bevares som forebyggende foranstaltning.', null, null, null, null, null, null, null, null, null, null);
