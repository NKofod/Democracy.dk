INSERT INTO ODA.dbo.Sag (id, typeid, kategoriid, statusid, titel, titelkort, offentlighedskode, nummer, nummerprefix, nummernumerisk, nummerpostfix, resume, afstemningskonklusion, periodeid, afgørelsesresultatkode, baggrundsmateriale, opdateringsdato, statsbudgetsag, begrundelse, paragrafnummer, paragraf, afgørelsesdato, afgørelse, rådsmødedato, lovnummer, lovnummerdato, retsinformationsurl, fremsatundersagid, deltundersagid) VALUES (54695, 10, 11, 20, N'Vil ministeren tage initiativ til en ændring eller præcisering af straffelovens § 119 om vold mod en person i offentlig tjeneste eller hverv, således at dømte efter denne paragraf som udgangspunkt altid straffes med mindst 60 dages ubetinget fængsel?', N'Om vold mod en person i offentlig tjeneste.', N'O', N'S 1321', N'S', N'1321', N'', N'', null, 25, null, null, N'2017-12-05 22:27:59.060', 0, N'Straffelovens § 119 handler om vold begået mod en person i offentlig tjeneste eller hverv, og det anses normalt for mere samfundsskadeligt at begå vold efter denne paragraf end mod »almindelige borgere«.
Forskellen kommer til udtryk ved, at Folketinget har bestemt, at strafferammen efter § 119 er fængsel på indtil 8 år, mens strafferammen efter den ''bløde'' voldsparagraf § 244 er fængsel indtil 3 år.
Imidlertid viser domspraksis, at der ofte slet ikke er forskel i den straf, som udmåles efter overtrædelse af § 119 i forhold til overtrædelse af § 244. Ofte er straffen 30 dages fængsel eller endda 30 dages betinget fængsel, hvis personen ikke tidligere er straffet. Det føler voldsofre efter § 119 sig med rette krænket over.
Spørgeren er af den opfattelse, at vold mod en person i offentlig tjeneste eller hverv (§ 119) som udgangspunkt skal udløse en straf, der er det dobbelte af det, der i dag sædvanligvis udmåles, dvs. en straf på mindst 60 dages ubetinget fængsel. Strafferammen på 8 års fængsel efter § 119 sammenholdt med 3 års fængsel efter § 244 tilsiger klart og tydeligt, at der bør udmåles en straf, der er godt det dobbelte af den straf, der udmåles efter § 244. Men sådan er det ikke.
Spørgeren vil lade det være op til ministeren, hvordan det sikres, at straffe efter § 119 som udgangspunkt bliver mindst 60 dages fængsel, blot det sker! Spørgeren ser frem til handling fra ministeren snarest.', null, null, null, null, null, null, null, null, null, null);
