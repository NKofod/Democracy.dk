INSERT INTO ODA.dbo.Sag (id, typeid, kategoriid, statusid, titel, titelkort, offentlighedskode, nummer, nummerprefix, nummernumerisk, nummerpostfix, resume, afstemningskonklusion, periodeid, afgørelsesresultatkode, baggrundsmateriale, opdateringsdato, statsbudgetsag, begrundelse, paragrafnummer, paragraf, afgørelsesdato, afgørelse, rådsmødedato, lovnummer, lovnummerdato, retsinformationsurl, fremsatundersagid, deltundersagid) VALUES (40255, 3, 13, 11, N'Forslag til lov om tilskud til distribution af dagblade.', N'Om tilskud til distribution af dagblade.', N'O', N'L 187', N'L', N'187', N'', N'Med loven skabes en ny ordning for direkte støtte til udgivere til distribution af dagblade i Danmark. Ordningen skal afløse den gældende bladtilskudsordning under Transport- og Energiministeriet med støtte via Post Danmark. 
Med loven oprettes en pulje under Kulturministeriet på 323 mio. kr., der svarer til det statslige tilskud i 2004 under den gældende bladtilskudsordning med støtte via Post Danmark. Udgivere kan i fremtiden selv vælge distributør, og Post Danmark kan selv vælge sine bladportotakster.
Kulturministeren nedsætter et sagkyndigt fordelingsudvalg med bl.a. repræsentanter for berørte faglige kredse, der træffer afgørelse om støttetildeling.
Loven er sidste led i en proces, der har indebåret en række ændringer af den gældende tilskudsordning.
