INSERT INTO ODA.dbo.Sag (id, typeid, kategoriid, statusid, titel, titelkort, offentlighedskode, nummer, nummerprefix, nummernumerisk, nummerpostfix, resume, afstemningskonklusion, periodeid, afgørelsesresultatkode, baggrundsmateriale, opdateringsdato, statsbudgetsag, begrundelse, paragrafnummer, paragraf, afgørelsesdato, afgørelse, rådsmødedato, lovnummer, lovnummerdato, retsinformationsurl, fremsatundersagid, deltundersagid) VALUES (15032, 7, null, 54, N'Forslag til Europa-Parlamentets og Rådets forordning (EU) Nr. .../... om tekniske krav til kreditoverførsler og direkte debiteringer i euro og om ændring af forordning (EF) nr. 924/2009 ', N'Forslag til forordning om om tekniske krav til kreditoverførsler og direkte debiteringer i euro mv.', N'O', N'KOM (2010) 0775', N'KOM', N'775', N'', N'<div>
<strong>Forslaget er modtaget</strong> af Folketinget i dansk sprogudgave den&nbsp;20. december&nbsp;2010, jf. <a href="http://www.euo.dk/upload/application/pdf/d83cf832/Protokol2_naerhed.pdf">Lissabontraktatens protokol nr. 2</a> om kontrol med nærhedsprincippet. <strong>Fristen </strong>på 8 uger for fremsendelse af Folketingets begrundede udtalelse <strong>udløb den&nbsp;14.&nbsp;februar&nbsp;2011.<br />
<br />
</strong><a href="/140091.docid">Læs</a> Kommissionens fremsendelsesbrev <br />
<br />
Tilknyttede dokumenter: SEK(2010) 1583, SEK(2010) 1584 og SEK(2010) 1585<br />
<br />
SEPA (the Single Euro Payment Area) er et EU-støttet initiativ fra den europæiske banksektor, der har til formål at gøre elektroniske eurobetalinger (f.eks. brug af betalingskort eller bankoverførsler) &nbsp;i EU eller EØS let tilgængelige og forbrugervenlige, Meningen er, at der ikke skal skelnes mellem indenlandske og grænseoverskridende transaktioner i euro i EU og EØS-landene hvad angår priser og øvrige vilkår.<br />
<br />
Siden 2008 har en lang række pengeinstitutter, herunder danske banker, haft mulighed for at udbyde en række SEPA-produkter.<br />
<br />
Kommissionen har fremsat et ændringsforslag til forordningen, som fastsætter slutdatoerne for den endelige overgang fra de forskellige nationale systemer til SEPA-løsningerne. Herefter vil SEPA-instrumenterne erstatte alle indenlandske instrumenter.<br />
<br />
SEPA-kreditoverførselsordningen (SCT) blev lanceret den 28. januar 2008. SEPA-ordningen for direkte debitering (SDD) blev lanceret den 2. november 2009, som den anden vigtige milesten i gennemførelsen af SEPA ved hjælp af ordninger, som omfatter hele EU.<br />
<br />
Resumeet er udarbejdet af Folketingets EU-Oplysning. For yderligere notater m.v. om forslaget se boksen med &quot;relaterede dokumenter&quot;. 
</div>', null, 11, null, null, N'2015-12-03 07:49:40.257', 0, null, null, null, null, null, null, null, null, null, null, null);
