INSERT INTO ODA.dbo.Dokument (id, typeid, kategoriid, statusid, offentlighedskode, titel, dato, modtagelsesdato, frigivelsesdato, paragraf, paragrafnummer, spørgsmålsordlyd, spørgsmålstitel, spørgsmålsid, procedurenummer, grundnotatstatus, dagsordenudgavenummer, opdateringsdato) VALUES (556739, 17, 22, 1, N'O', N'L 197 - svar på spm. 5, fra skatteministeren', N'2009-05-13 00:00:00.000', N'2009-05-13 00:00:00.000', N'2009-05-13 10:04:51.000', N'', N'', N'Kan ministeren bekræfte, at reglerne er som beskrevet nedenfor, og at man skattemæssigt har modregning af underskud af virksomhed i lønindkomst, (der forudsætter at virksomheden ikke er hobby), mens det samme ikke er tilfælde ved beregning af arbejdsmarkedsbidrag:

Arbejdsmarkedsbidrag
En iværksætter som i et indkomstår starter virksomhed, som i år 1 giver underskud, og derudover i samme indkomstår har lønindkomst, kan ikke modregne det betalte arbejdsmarkedsbidrag fra lønindkomsten i det negative arbejdsmarkedsbidrag fra underskud af virksomhed, og herudover kan det negative arbejdsmarkedsbidrag heller ikke fremføres til senere modregning i f.eks. overskud af virksomhed i år 2. 

Skat
Underskud af virksomhed kan fratrækkes i anden personlig indkomst, idet det forudsættes at virksomheden ikke er en hobbyvirksomhed.', N'Spm. om ministeren kan bekræfte, at reglerne er som beskrevet, og at man skattemæssigt har modregning af underskud af virksomhed i lønindkomst, til skatteministeren', 556738, N'', N'', null, N'2015-12-05 21:09:14.063');
