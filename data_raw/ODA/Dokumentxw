INSERT INTO ODA.dbo.Dokument (id, typeid, kategoriid, statusid, offentlighedskode, titel, dato, modtagelsesdato, frigivelsesdato, paragraf, paragrafnummer, spørgsmålsordlyd, spørgsmålstitel, spørgsmålsid, procedurenummer, grundnotatstatus, dagsordenudgavenummer, opdateringsdato) VALUES (545972, 17, 22, 1, N'O', N'UDU alm. del - svar på spm. 177, fra undervisningsministeren', N'2009-03-20 00:00:00.000', N'2009-03-20 00:00:00.000', N'2009-04-16 15:51:19.000', N'', N'', N'Agter ministeren at sikre, at den nedenfor omtalte skoleleder ikke bliver fyret eller på anden måde bliver presset væk fra sin stilling i Københavns Kommune på baggrund af sine udtalelser, og kan ministeren oplyse, hvad kommunen har foretaget sig overfor den pågældende skoleleder, og er ministeren enig med spørgeren i, at det er forrykt, såfremt Københavns Kommune undertrykker sine medarbejderes ytringsfrihed og mulighed for at ytre sig om de tendenser, som er i det danske samfund, herunder folkeskolen?

Muslimske børn er mere voldelige og kriminelle og muslimer står i højere grad for en volds og trusselskultur. De udtalelser kommer fra en københavnsk skoleleder på Ålholm Skole i Valby. Skolelederen har udtalt sig om kulturforskelle, og har ikke sagt, at alle muslimer er voldelige osv. Hans arbejdsgiver Københavns Kommune har nu indkaldt skolelederen til en tjenestelig samtale efter sigende, fordi skolelederen har udtalt sig i strid med kommunens værdigrundlag. Spørgeren ønsker, at ministeren oplyser, hvordan den pågældende skolele-der er stillet i drøftelserne med kommunen. Spørgeren ønsker, at ministeren retter henven-delse til kommunen og indskærper, at det er regeringens holdning, at kommunen skal udvise en passende respekt for sine medarbejdere.
Der henvises også til indslag fra TV2Lorry fra henholdsvis den 10. og 11. februar 2009: 

http://tv2lorry.dk/moduler/nyheder/showregvideo.asp?dato=10-02-2009&cID=1&vId=468573

og

http://tv2lorry.dk/moduler/nyheder/showregvideo.asp?dato=11-02-2009&cID=1&vId=468812
', N'Spm. om en skoleleders udtalelser om muslimske børn, til undervisningsministeren', 545971, N'', N'', null, N'2015-12-05 20:44:23.723');
