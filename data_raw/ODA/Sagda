INSERT INTO ODA.dbo.Sag (id, typeid, kategoriid, statusid, titel, titelkort, offentlighedskode, nummer, nummerprefix, nummernumerisk, nummerpostfix, resume, afstemningskonklusion, periodeid, afgørelsesresultatkode, baggrundsmateriale, opdateringsdato, statsbudgetsag, begrundelse, paragrafnummer, paragraf, afgørelsesdato, afgørelse, rådsmødedato, lovnummer, lovnummerdato, retsinformationsurl, fremsatundersagid, deltundersagid) VALUES (34023, 10, 11, 20, N'Vil ministeren for hvert enkelt amt samt for Frederiksberg, Københavns og Bornholms Kommuner oplyse, hvilken frist der efter retssikkerhedslovens § 3, stk. 2, er fastsat for behandling af ansøgninger om reparation af særlige indretninger i handicapbiler, og om de nævnte frister i givet fald regnes fra aflevering af ansøgningen eller fra amtets modtagelse af ansøgningen, idet der efter reglerne også forudsættes en forudgående sagsbehandling i ansøgerens kommune?', N'Om ansøgninger til reparationer af handicapbiler.', N'O', N'S 2327', N'S', N'2327', N'', N'', null, 21, null, null, N'2017-12-07 18:44:10.103', 0, N'Ministeren har besvaret spørgerens generelle spørgsmål nr. S 1868 med en meget konkret udtalelse fra Århus Amt, hvoraf fremgår, at der i dette amt tilstræbes en sagsbehandlingstid på 5 dage ved ansøgning om reparation af særlige indretninger.
Ministeren oplyser imidlertid ikke, om denne frist er fastsat med hjemmel i retssikkerhedslovens § 3, stk. 2, og navnlig oplyser ministeren ikke, hvilke frister der i givet fald er fastsat i de øvrige amter. Dette ønsker derfor oplyst.
Det kan lyde som en besnærende kort sagsbehandlingstid, hvis en afgørelse kan træffes af et amt i løbet af kun 5 dage. 
Men for det første forudsættes der ved alle sager om handicapbiler en forudgående sagsbehandling i ansøgerens kommune, således at den reelle sagsbehandlingstid typisk vil være meget længere end de nævnte 5 dage. 
For det andet er også 5 dage meget lang tid at undvære sin transportmulighed, f.eks. hvis man i så lang en periode er afskåret fra at passe sit arbejde, uden at det skyldes sygdom. 
For det tredje er risikoen for misbrug af reglerne yderst beskeden, idet det er utænkeligt, at man som ejer af en handicapbil skulle få den ide at lade en særlig indretning reparere, hvis der ikke er nogen grund til det, og derfor burde de bevilgende myndigheder kunne vise brugerne den tillid, at en reparation kan foretages uden en forudgående sagsbehandling i hele to instanser.
