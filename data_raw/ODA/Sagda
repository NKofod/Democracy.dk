INSERT INTO ODA.dbo.Sag (id, typeid, kategoriid, statusid, titel, titelkort, offentlighedskode, nummer, nummerprefix, nummernumerisk, nummerpostfix, resume, afstemningskonklusion, periodeid, afgørelsesresultatkode, baggrundsmateriale, opdateringsdato, statsbudgetsag, begrundelse, paragrafnummer, paragraf, afgørelsesdato, afgørelse, rådsmødedato, lovnummer, lovnummerdato, retsinformationsurl, fremsatundersagid, deltundersagid) VALUES (34292, 10, 11, 20, N'Vil ministeren redegøre for, i hvilket omfang der er belæg for, at de skattelettelser, der blev gennemført i forbindelse med Forårspakken i 2004, har givet udslag i øget aktivitet i økonomien, samt oplyse størrelsen af denne aktivitet målt i BNP-vækst og antal arbejdspladser, og såfremt skattelettelserne har bidraget positivt til væksten, vurdere, om disse skattelettelser har haft indflydelse på borgernes adfærd, for så vidt angår ønsket om at yde en ekstra indsats?', N'Om Forårspakken i 2004', N'O', N'S 2565', N'S', N'2565', N'', N'', null, 21, null, null, N'2017-12-07 18:44:54.620', 0, N'I 2003 vedtog regeringen og Dansk Folkeparti en række skattelettelser i »Lavere skat på arbejdsindkomst«, som bl.a. skulle hæve mellemskattegrænsen hvert år frem til 2007. I 2004 fremlagde regeringen Forårspakken. Her suspenderedes den særlige pensionsopsparing, og man fremrykkede de skattelettelser, der allerede var besluttet, således at de fik virkning med tilbagevirkende kraft fra og med 1. januar 2004. Forårspakken blev gennemført sammen med Dansk Folkeparti. Formålet var at fremskynde det allerede begyndende opsving i dansk økonomi.
Sammenlignet med de fleste europæiske lande klarer dansk økonomi sig utroligt godt. Arbejdsløsheden ligger omkring 6 pct., mens den i flere eurolande er over 10 pct. Danmark har endvidere store overskud på det offentlige budget, mens de fleste eurolande har store underskud. At dansk økonomi klarer sig markant bedre end eurolandene er en konsekvens af en længere række faktorer, som de strukturelle reformer, skiftende regeringer har gennemført gennem, mere end to årtier, hvorimod flere af eurolandene af forskellige årsager har undladt at tilpasse deres samfund til de øgede krav, som globaliseringen stiller til et moderne samfund. Der er ikke nogen tvivl om, at dansk økonomi ville have klaret sig langt bedre end f.eks. den tyske økonomi, selv om vi ikke havde gennemført skattelettelserne i 2003 og 2004. Men der er efter spørgerens vurdering heller ikke nogen tvivl om, at initiativerne i disse år har haft stor betydning for det økonomiske opsving, som vi oplever nu. Spørgeren ønsker således ministerens vurdering af effekten af Forårspakkens indvirkning på aktiviteten i økonomien og dens effekt på borgernes ønske om at yde en ekstra indsats. Det er spørgerens håb, at Forårspakken og andre tilsvarende initiativer i fremtiden kan have den effekt, at borgerne til stadighed motiveres til og belønnes for at yde en ekstra indsats.
', null, null, null, null, null, null, null, null, null, null);
