INSERT INTO ODA.dbo.Sag (id, typeid, kategoriid, statusid, titel, titelkort, offentlighedskode, nummer, nummerprefix, nummernumerisk, nummerpostfix, resume, afstemningskonklusion, periodeid, afgørelsesresultatkode, baggrundsmateriale, opdateringsdato, statsbudgetsag, begrundelse, paragrafnummer, paragraf, afgørelsesdato, afgørelse, rådsmødedato, lovnummer, lovnummerdato, retsinformationsurl, fremsatundersagid, deltundersagid) VALUES (18995, 7, null, 54, N'Forslag til Europa-Parlamentets og Rådets forordning (EF) Nr. .../... om foranstaltninger til opretholdelse af naturgasforsyningssikkerheden og ophævelse af direktiv 2004/67/EF ', N'Forslag til forordning om om foranstaltninger til opretholdelse af naturgasforsyningssikkerheden og ophævelse af direktiv 2004/67/EF', N'O', N'KOM (2009) 0363', N'KOM', N'363', N'', N'Tillæg: <a href="http://register.consilium.europa.eu/pdf/en/09/st11/st11892-ad01.en09.pdf" onclick="this.target=''_blank''">SEK (2009) 977</a>, <a href="http://register.consilium.europa.eu/pdf/en/09/st11/st11892-ad02.en09.pdf" onclick="this.target=''_blank''">SEK (2009) 978</a>, <a href="http://register.consilium.europa.eu/pdf/en/09/st11/st11892-ad03.en09.pdf" onclick="this.target=''_blank''">SEK (2009) 979</a> og <a href="http://register.consilium.europa.eu/pdf/da/09/st11/st11892-ad04.da09.pdf" onclick="this.target=''_blank''">SEK (2009) 980</a><br />
<br />
Kommissionen har fremsat sit forslag i lyset af den russisk-ukrainske gaskrise i januar 2009&nbsp;og som svar på opfordringen fra Det Europæiske Råd til at foretage en fremskyndet revision&nbsp;af det eksisterende direktiv om foranstaltninger til opretholdelse af naturgasforsyningssikkerheden, så EU kan være bedre forberedt i tilfælde af en eventuel fremtidig afbrydelse af gasforsyningen. <br />
<br />
Forslaget tager i denne henseende navnlig sigte på at definere alle involverede aktørers, herunder gasindustriens, medlemsstaternes og Fællesskabets institutioners, roller og ansvar mere tydeligt med henblik på at håndtere en afbrydelse af forsyningerne på kort sigt og skabe den nødvendige infrastruktur på længere sigt.<br />
