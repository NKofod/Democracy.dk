INSERT INTO ODA.dbo.Sag (id, typeid, kategoriid, statusid, titel, titelkort, offentlighedskode, nummer, nummerprefix, nummernumerisk, nummerpostfix, resume, afstemningskonklusion, periodeid, afgørelsesresultatkode, baggrundsmateriale, opdateringsdato, statsbudgetsag, begrundelse, paragrafnummer, paragraf, afgørelsesdato, afgørelse, rådsmødedato, lovnummer, lovnummerdato, retsinformationsurl, fremsatundersagid, deltundersagid) VALUES (19390, 7, null, 54, N'Meddelelse fra Kommissionen til Europa-Parlamentet, Rådet, Det Europæiske Økonomiske og Sociale Udvalg og Regionsudvalget: Samhørighedspolitik: Strategirapport 2010 om gennemførelsen af programmerne for perioden 2007-2013', N'Strategirapport 2010 om gennemførelsen af programmerne for perioden 2007-2013', N'O', N'KOM (2010) 0110', N'KOM', N'110', N'', N'Tillæg: <a href="http://register.consilium.europa.eu/pdf/en/10/st08/st08321-ad01.en10.pdf">SEK (2010) 360</a><br />
<br />
Kommissionens meddelelse giver et første oversigtsbillede over gennemførelsen af de samhørighedspolitiske programmer for perioden 2007-2013. Meddelelsen er hovedsagelig baseret på de 27 nationale strategirapporter, som er et nyt element i samhørighedspolitikken for denne periode.<br />
<br />
De nationale strategirapporter blev indført for at skabe større gennemsigtighed og tilskynde til ansvarlighed for politikken i medlemsstaterne som led i den delte forvaltning af samhørighedspolitikken. De nationale strategirapporter er et vigtigt redskab til at overvåge gennemførelsen af medlemsstaternes strategiske forpligtelser til at gennemføre EU''s mål på højt niveau ved hjælp af samhørighedspolitikken.<br />
<br />
Meddelelsen skal skabe en bedre debat med EU''s institutioner, da samhørighedspolitikken har en væsentlig indflydelse på arbejdet med at skabe en bæredygtig økonomisk og social udvikling i Europas regioner og medlemsstater. På denne baggrund opstilles der i det afsluttende afsnit en række anbefalinger til, hvorledes man kan forbedre den effektive gennemførelse af programmerne med henblik på at maksimere 2007-2013-programmernes bidrag til at styrke peer review og policy-læring og samtidig styrke deres bidrag til EU 2020-strategien.<br />
<br />
Resumeet er udarbejdet af Folketingets EU-Oplysning. For yderligere notater m.v. om forslaget se boksen med &quot;relaterede dokumenter&quot;.', null, 11, null, null, N'2015-12-03 07:27:47.230', 0, null, null, null, null, null, null, null, null, null, null, null);
INSERT INTO ODA.dbo.Sag (id, typeid, kategoriid, statusid, titel, titelkort, offentlighedskode, nummer, nummerprefix, nummernumerisk, nummerpostfix, resume, afstemningskonklusion, periodeid, afgørelsesresultatkode, baggrundsmateriale, opdateringsdato, statsbudgetsag, begrundelse, paragrafnummer, paragraf, afgørelsesdato, afgørelse, rådsmødedato, lovnummer, lovnummerdato, retsinformationsurl, fremsatundersagid, deltundersagid) VALUES (19391, 7, null, 54, N'Meddelelse fra Kommissionen til Rådet og Europa-Parlamentet: EU-rammepolitik for bistand til løsning af udviklingslandenes fødevaresikkerhedsproblemer', N'Meddelelse om EU-rammepolitik for bistand til løsning af udviklingslandenes fødevaresikkerhedsproblemer', N'O', N'KOM (2010) 0127', N'KOM', N'127', N'', N'', null, 11, null, null, N'2015-12-03 07:31:42.437', 0, null, null, null, null, null, null, null, null, null, null, null);
INSERT INTO ODA.dbo.Sag (id, typeid, kategoriid, statusid, titel, titelkort, offentlighedskode, nummer, nummerprefix, nummernumerisk, nummerpostfix, resume, afstemningskonklusion, periodeid, afgørelsesresultatkode, baggrundsmateriale, opdateringsdato, statsbudgetsag, begrundelse, paragrafnummer, paragraf, afgørelsesdato, afgørelse, rådsmødedato, lovnummer, lovnummerdato, retsinformationsurl, fremsatundersagid, deltundersagid) VALUES (19392, 7, null, 54, N'Meddelelse fra Kommissionen til Rådet og Europa-Parlamentet: Humanitær fødevarebistand', N'Meddelelse om humanitær fødevarebistand', N'O', N'KOM (2010) 0126', N'KOM', N'126', N'', N'Tillæg: <a href="http://register.consilium.europa.eu/pdf/en/10/st08/st08250-ad01.en10.pdf" onclick="this.target=''_blank''">SEK (2010) 374</a><br />
<br />
Rådet, Europa-Parlamentet og Kommissionen vedtog i december 2007 en <a href="http://eur-lex.europa.eu/LexUriServ/LexUriServ.do?uri=OJ:C:2008:025:0001:0012:DA:PDF" onclick="this.target=''_blank''">konsensus om humanitær bistand</a>. Med vedtagelsen af denne erklæring forpligter de tre institutioner sig til at arbejde for fremme af effektiviteten af EU''s humanitære bistand. Kommissionen ville som følge heraf udarbejde en handlingsplan for de praktiske foranstaltninger til gennemførelse af denne erklæring. <br />
<br />
Det overordnede mål med denne meddelelse er at gøre EU''s fødevarebistand så effektiv som mulig og at skabe øget sammenhæng mellem de forskellige politikker og en bedre koordinering af leveringen af fødevarebistand.<br />
<br />
Den nye politik for humanitær fødevarebistand, som supplerer politikken for fødevaresikkerhed, KOM (2010) 0127, indeholder målene, principperne og strategierne for EU''s indsats for at løse akutte problemer vedrørende fødevareusikkerhed og underernæring i krisesituationer. Der fokuseres på at nå ud til de ernæringsmæssigt mest sårbare befolkningsgrupper i nødsituationer, særlig kvinder og børn, og på at sikre kriseramte samfunds evne til at brødføde sig selv på både kort og længere sigt.<br />
