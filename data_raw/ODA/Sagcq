INSERT INTO ODA.dbo.Sag (id, typeid, kategoriid, statusid, titel, titelkort, offentlighedskode, nummer, nummerprefix, nummernumerisk, nummerpostfix, resume, afstemningskonklusion, periodeid, afgørelsesresultatkode, baggrundsmateriale, opdateringsdato, statsbudgetsag, begrundelse, paragrafnummer, paragraf, afgørelsesdato, afgørelse, rådsmødedato, lovnummer, lovnummerdato, retsinformationsurl, fremsatundersagid, deltundersagid) VALUES (28970, 10, 11, 20, N'Vil ministeren oplyse, om der i Civilretsdirektoratet er forståelse for, at der kan være behov for i nogle tilfælde at tillade en mere liberal praksis over for fonde, der søger om tilladelse til at nedsætte den bundne kapital ved at overføre midler til fri kapital og derved skabe bedre mulighed for udlodninger i overensstemmelse med fundatsens formål?', N'Om bunden kapital for fonde.', N'O', N'S 432', N'S', N'432', N'', N'', null, 20, null, null, N'2017-12-08 14:58:05.810', 0, N'Mange velgørende, mindre fonde har vanskeligt ved at opfylde deres formål. Det kan skyldes, at kredsen af personer, der i henhold til fundatsen kan begunstiges, er lille, men det kan også skyldes, at næsten hele fondens formue er bundne midler, der som udgangspunkt er urørlig. 
I perioder, hvor renteniveauet er faldet gennem en årrække, vil afkastet i mange fonde være faldende med den virkning, at en mindre formue ikke er tilstrækkelig til at foretage udlodninger til den personkreds, der egentlig var betænkt af stifteren. Det er naturligvis i strid med legatstifterens hensigter, hvis det forholder sig sådan, og det rører naturligt ved spørgsmålet, om det ikke ville være bedre i overensstemmelse med stifterens hensigter at tillade overførsel af en del af grundkapitalen til frie midler med henblik på udlodning. 
Spørgeren er bekendt med en brancheunderstøttelsesfond, der er oprettet før år 1900, og som har en formue på ca. 1,1 mio. kr. og en kreds på ca. 30 potentielle legatmodtagere. Men en så lille formue afkaster ikke nok til, at der kan udloddes legater, idet administrations-, bank-, og revisionsudgifter beslaglægger hele indtægten.  Fondsbestyrelsen kan overveje at søge fonden lagt sammen med en anden fond eller søge den opløst, men det ville ikke give nogen mening i forhold til grundlæggernes ønsker. Det ville formentlig være langt bedre i overensstemmelse med grundlæggernes ønsker, hvis en del af grundkapitalen – måske på sigt det meste af eller hele grundkapitalen - kunne overføres til frie midler og uddeles til den personkreds, der er begunstiget i fundatsen.
Spørgeren er orienteret om, at den pågældende fond i første omgang ikke har fundet forståelse for sit synspunkt hos Civilretsdirektoratet og er derfor interesseret i at høre, om der i Civilretsdirektoratet er en generel forståelse for det her omtalte problem, og om direktoratet i konkrete tilfælde indgår i en positiv dialog med fondene om at løse problemer af denne karakter.', null, null, null, null, null, null, null, null, null, null);
