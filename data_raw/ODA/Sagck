INSERT INTO ODA.dbo.Sag (id, typeid, kategoriid, statusid, titel, titelkort, offentlighedskode, nummer, nummerprefix, nummernumerisk, nummerpostfix, resume, afstemningskonklusion, periodeid, afgørelsesresultatkode, baggrundsmateriale, opdateringsdato, statsbudgetsag, begrundelse, paragrafnummer, paragraf, afgørelsesdato, afgørelse, rådsmødedato, lovnummer, lovnummerdato, retsinformationsurl, fremsatundersagid, deltundersagid) VALUES (26600, 7, null, 54, N'Forslag til RÅDETS AFGØRELSE om finansiel støtte fra Unionen til Irland SEK(2010) 1516 endelig', N'Forslag til RÅDETS AFGØRELSE om finansiel støtte fra Unionen til Irland SEK(2010) 1516 endelig', N'O', N'KOM (2010) 0730', N'KOM', N'730', N'', N'Tilhørende dokument: <a href="http://eur-lex.europa.eu/LexUriServ/LexUriServ.do?uri=SEC:2010:1516:FIN:EN:PDF" onclick="this.target=''_blank''">SEK(2010) 1516</a><br />
&nbsp;<br />
Irland anmodede i november 2010 de øvrige EU-lande og IMF om låneassistance med henblik på at genoprette den finansielle sektor, sikre den finansielle stabilitet og understøtte de offentlige finanser. &nbsp;<br />
&nbsp; <br />
Forslaget lægger op til, at EU yder et EFSM-lån til Irland på 22,5 mia. euro, som led i et samlet låneprogram på 85 mia. euro, som finansieres via IMF, EU-mekanismen EFSM, eurozonefaciliteten EFSF, samt bilaterale lån fra Danmark, UK og Sverige. Irland vil desuden selv bidrage til programmet med 17,5 mia. euro ved hjælp af overskydende likviditet, som Irland aktuelt har til rådighed fra tidligere statsobligationsudstedelser.<br />
&nbsp;<br />
Danmarks yder et et bilateralt lån på 0,4 mia. euro (ca. 3 mia. kr.). Til sammenligning bliver UK&rsquo;s og Sveriges bilaterale lån på hhv. ca. 3,8 mia. euro og ca. 0,6 mia. euro.<br />
&nbsp;<br />
Resumeet er udarbejdet af Folketingets EU-Oplysning. For yderligere notater m.v. om forslaget se boksen med &quot;relaterede dokumenter&quot;.', null, 11, null, null, N'2015-12-03 09:27:57.680', 0, null, null, null, null, null, null, null, null, null, null, null);
