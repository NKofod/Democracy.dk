INSERT INTO ODA.dbo.Sag (id, typeid, kategoriid, statusid, titel, titelkort, offentlighedskode, nummer, nummerprefix, nummernumerisk, nummerpostfix, resume, afstemningskonklusion, periodeid, afgørelsesresultatkode, baggrundsmateriale, opdateringsdato, statsbudgetsag, begrundelse, paragrafnummer, paragraf, afgørelsesdato, afgørelse, rådsmødedato, lovnummer, lovnummerdato, retsinformationsurl, fremsatundersagid, deltundersagid) VALUES (37860, 10, 11, 20, N'Vil ministeren oplyse, hvad forskellen er på det sikkerhedsministerium, som EU under navnet COSI (Standing Committee on Internal Security) er ved at etablere, og det organ, som skulle have været oprettet med hjemmel i EU-forfatningens artikel 261?', N'Om oprettelse af et EU-ministerium for sikkerhed og politi.', N'O', N'S 1613', N'S', N'1613', N'', N'', null, 22, null, null, N'2017-12-07 17:54:33.797', 0, N'EU-forfatningen indeholder det retlige grundlag for COSI i artikel III-261, som lyder: »Der nedsættes en stående komité i Rådet, der skal sikre, at det operationelle samarbejde om den indre sikkerhed fremmes og styrkes i Unionen«. Som bekendt er EU-forfatningen død, og EU-borgerne er lovet en tænkepause. Alligevel ser det ud til, at man i EU er ved at oprette et COSI.
For spørgeren er det naturligvis dybt kritisabelt, at EU nu for anden gang på under et halvt år forsøger at trumfe EU-forfatningen igennem. Første gang, vi så det, var ved dommen C176/03, der flyttede strafferetskompetencen til søjle I og dermed gjorde det til et område for flertalsafgørelser, ligesom forfatningens artikel 271 ville have gjort det. Og nu er det altså artikel 261, som man - på trods af befolkningernes afvisning - vil gennemføre.
Ministeren bedes derfor redegøre for, om det ikke er rigtigt, at man ved beslutningen om at oprette denne særlige komité indfører det, der var tiltænkt med forfatningens artikel 261. I bekræftende fald, bedes hun endvidere meddele, hvad hendes holdning er til dette. Og i benægtende fald, bedes hun redegøre for, hvad forskellen på det, man nu indfører, og det, der ville være indført med EU-forfatningen, er.', null, null, null, null, null, null, null, null, null, null);
