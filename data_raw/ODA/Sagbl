INSERT INTO ODA.dbo.Sag (id, typeid, kategoriid, statusid, titel, titelkort, offentlighedskode, nummer, nummerprefix, nummernumerisk, nummerpostfix, resume, afstemningskonklusion, periodeid, afgørelsesresultatkode, baggrundsmateriale, opdateringsdato, statsbudgetsag, begrundelse, paragrafnummer, paragraf, afgørelsesdato, afgørelse, rådsmødedato, lovnummer, lovnummerdato, retsinformationsurl, fremsatundersagid, deltundersagid) VALUES (16328, 7, null, 54, N'Meddelelse fra Kommissionen til Europa-Parlamentet,  Det Europæiske Råd, Rådet, Den Europæiske Centralbank, Det Europæiske Økonomiske og Sociale Udvalg og Regionsudvalget om øget samordning af de økonomiske politikker med henblik på stabilitet, vækst og beskæftigelse – redskaber til styrkelse af den økonomiske styring i EU ', N'Øget samordning af de økonomiske politikker med henblik på stabilitet, vækst og beskæftigelse – redskaber til styrkelse af den økonomiske styring i EU ', N'O', N'KOM (2010) 0367', N'KOM', N'367', N'', N'<p>
NB <a href="/135124.docid">Se også KOM (2010) 0250</a> - Styrket samordning af de økonomiske politikker 
</p>

<p>
Forslagets formål er, at sikre en strammere økonomisk og finanspolitisk koordinering i lyset af de store offentlige underskud og den store offentlige gæld, især i Grækenland, Spanien, Portugal og Irland.&nbsp;<br />
<br />
Koordinationen sikres gennem indførelsen af et såkaldt &rdquo;europæisk semester&rdquo;, som sikrer koordination på EU-plan i foråret inden landene selv vedtager deres årlige finanslove.&nbsp;<br />
<br />
Det &rdquo;Europæiske Semester&rdquo; vil dække alle elementer af den økonomiske overvågning, og Stabilitets- og konvergensprogrammerne (SCP) og de Nationale Reformprogrammer (NRP) vil blive afleveret på samme tid og vurderet samtidig af Kommissionen. De fastholdes dog som separate instrumenter.<br />
<br />
Forslaget rummer også Kommissionens overvågning af makroøkonomiske ubalancer. dvs. alle de økonomiske nøgletal og parametre, der ikke direkte omhandler finansielle poster. Det drejer sig om vækstrater (BNP), inflation, beskæftigelse, lønstigninger, konkurrenceevne m.v. overvågningen vil også omfatte EU-landenes strukturreformer på baggrund af de Nationale Reform-programmer.&nbsp;<br />
<br />
Hvis der er optræk til, at der opstår uholdbare ubalancer kan Rådet på forslag fra Kommissionen sætte et land under streng overvågning. . Det pågældende EU-land vil skulle rapportere regelmæssigt til økofinrådet og euro-gruppen om fremskridtene mht. implementering af de anbefalede reformer. Der vil særlig &rdquo;hårde&rdquo; regler på dette område, for de lande, der er medlemmer af euroen.&nbsp;<br />
<br />
Der vil ikke være egentlige økonomiske sanktioner forbundet med den makroøkonomiske overvågning og overholdelse heraf.&nbsp;<br />
<br />
Meddelelsen lægger desuden op til øget fokus på offentlig gæld og finanspolitisk holdbarhed. Formålet er navnlig, at få nedbragt uforholdsmæssige store underskud på en mere effektiv måde end hidtil. Det gælder også spørgsmålet om nedbringelse af for stor offentlig gæld.&nbsp;<br />
<br />
Resumeet er udarbejdet af Folketingets EU-Oplysning. For yderligere notater m.v. om forslaget se boksen med &quot;relaterede dokumenter&quot;. 
</p>', null, 11, null, null, N'2015-12-03 07:36:03.797', 0, null, null, null, null, null, null, null, null, null, null, null);
