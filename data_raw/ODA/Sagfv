INSERT INTO ODA.dbo.Sag (id, typeid, kategoriid, statusid, titel, titelkort, offentlighedskode, nummer, nummerprefix, nummernumerisk, nummerpostfix, resume, afstemningskonklusion, periodeid, afgørelsesresultatkode, baggrundsmateriale, opdateringsdato, statsbudgetsag, begrundelse, paragrafnummer, paragraf, afgørelsesdato, afgørelse, rådsmødedato, lovnummer, lovnummerdato, retsinformationsurl, fremsatundersagid, deltundersagid) VALUES (79295, 3, 13, 11, N'Forslag til lov om ændring af lov om friskoler og private grundskoler m.v., lov om efterskoler og frie fagskoler og lov om private institutioner for gymnasiale uddannelser. (Øgede frihedsgrader, rammer for donationer og styrket tilsyn med de frie skoler m.v.).', N'Om øgede frihedsgrader, rammer for donationer og styrket tilsyn med de frie skoler m.v.', N'O', N'L 218', N'L', N'218', N'', N'Lovforslaget udmønter aftale af 25. januar 2019 mellem regeringen (Venstre, Liberal Alliance og Det Konservative Folkeparti) og Dansk Folkeparti om bedre rammer for og styrket tilsyn med de frie skoler.

Lovforslaget indebærer, at frie grundskoler og private gymnasier får frihed til at oprette skoletilbud i en afdeling af en fri skole, hvis afdelingen er beliggende i hovedskolens kommune eller i en nabokommune til hovedskolens beliggenhedskommune. 

Lovforslaget præciserer, at frie grundskoler får mulighed for at oprette klubtilbud for elever fra 4. klassetrin.

Lovforslaget indebærer et generelt forbud mod anonyme donationer til de frie skoler.

Lovforslaget indfører for efterskoler, frie fagskoler og private gymnasier en regulering svarende til de gældende bestemmelser for frie grundskolers frihed og folkestyre-krav, mulighed for at tilsynet kan deltage som observatør på møder på skolen, et styrket tilsyn med skolernes leje af bygninger, en whistleblowerordning og en vurdering af personkredsen ved oprettelse af en ny skole. 

Lovforslaget indeholder også en række andre ændringer.

', N'Forslaget blev vedtaget. For stemte 90 (S, DF, V, LA, RV, SF og KF), imod stemte 15 (EL og ALT), hverken for eller imod stemte 0.', 148, null, null, N'2019-05-10 10:02:20.280', 0, null, null, null, null, null, null, N'563', N'2019-05-07 00:00:00.000', null, null, null);
INSERT INTO ODA.dbo.Sag (id, typeid, kategoriid, statusid, titel, titelkort, offentlighedskode, nummer, nummerprefix, nummernumerisk, nummerpostfix, resume, afstemningskonklusion, periodeid, afgørelsesresultatkode, baggrundsmateriale, opdateringsdato, statsbudgetsag, begrundelse, paragrafnummer, paragraf, afgørelsesdato, afgørelse, rådsmødedato, lovnummer, lovnummerdato, retsinformationsurl, fremsatundersagid, deltundersagid) VALUES (79296, 3, 13, 13, N'Forslag til lov om ændring af skatteforvaltningsloven. (Aftale om en ny ankenævnsstruktur og en hurtigere vej gennem klagesystemet – retssikkerhedspakke IV).', N'Om en ny skatteankenævnsstruktur og en hurtigere vej gennem klagesystemet.', N'O', N'L 212', N'L', N'212', N'', N'Formålet med lovforslaget er at bidrage til at nedbringe sagsbehandlingstiden i klagesystemet, som er udfordret af lange sagsbehandlingstider.

Det foreslås at ændre fordelingen af sagstyperne mellem skatteankenævn og Landsskatteretten, så sagstyperne målrettes afgørelsesmyndighedernes kompetencer. Fremadrettet skal skatteankenævnene f.eks. tage sig af sager om momsskøn, mens mere komplekse sager om f.eks. told, afgifter og moms behandles i Landsskatteretten.

Desuden foreslås det at afskaffe fritvalgsordningen, der har gjort det muligt for en klager frit at vælge, om en sag, der er visiteret til et skatteankenævn, i stedet skal behandles af Landsskatteretten. Til gengæld foreslås det, at den skattepligtige skal have mulighed for at indbringe sin klage direkte for domstolene uden først at indbringe den for klagemyndigheden.

Lovforslaget skal endvidere ses i sammenhæng med en ny struktur for skatteankenævnene, som skal bevare og fremtidssikre et stærkt lægmandselement med fokus på lokal forankring. Det suppleres med, at der udpeges 10 medlemmer med skattefaglig indsigt og relevant uddannelse.

