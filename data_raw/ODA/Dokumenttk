INSERT INTO ODA.dbo.Dokument (id, typeid, kategoriid, statusid, offentlighedskode, titel, dato, modtagelsesdato, frigivelsesdato, paragraf, paragrafnummer, spørgsmålsordlyd, spørgsmålstitel, spørgsmålsid, procedurenummer, grundnotatstatus, dagsordenudgavenummer, opdateringsdato) VALUES (459543, 17, 22, 3, N'O', N'AMU L 155 - svar på spm. 8, fra familie- og forbrugerministeren', N'2006-05-09 00:00:00.000', N'2006-05-09 00:00:00.000', N'2006-05-09 15:59:34.820', N'', N'', N'Ministeren skriver i svaret på spm 4, at der ikke har stået noget på ligestillingsministerens hjemmeside om, hvorvidt forældrene har ret til at dele barslen, som de vil, eller om dette forudsætter en aftale med arbejdsgiveren. Kan ministeren bekræfte, at der på følgende webadresse http://www.lige.dk/Default.asp?Id=134&AjrNws=606&AjrNwsPg=1 dateret d. 23.09.05 står: 

Usikkerhed om barselsreglerne

I en dom fra Østre Landsrets af 22. september blev det udtrykt, at der ikke var tilstrækkelig hjemmel til at forældrene kan dele barselsorloven frit imellem sig sådan at ugens dage deles mellem forældrene. 

Regeringens intention med reglerne om barselsorlov var, at forældrene skulle have mulighed for selv at afgøre, hvordan orloven skulle fordeles, herunder også orlov på skift og på deltid. 

Ministeren for Familie- og forbrugeranliggender er derfor nu ved at gennemgå dommen for at se, om den får konsekvenser og evt. indebærer præciseringer af den gældende lovgivning."

Mener ministeren ikke, at ordvalget "forældrene selv" hentyder til, at det ikke kræver en aftale med arbejdsgiver, men at det er en beslutning, som forældrene træffer suverænt og dermed en rettighed? Og kan ovenstående ikke udelukkende tolkes derhen, at ministeren vil ændre lovgivningen således, at forældrene sikres denne mulighed? Hvad har gennemgangen af dommen vist, hvis den da er afsluttet (siden er som sagt dateret 23.09.05 )? Agter ministeren at tage intiativ til en præcisering af lovgivningen, såfremt Højesteret stadfæster Østre Landsrets dom, således at den oprindeligt tiltænkte rettighed bliver lovfæstet?', N'Spm. om forældrenes mulighed for selv at fordele barslen, jf. ligestillingsministerens hjemmeside, til familie- og forbrugerministeren', 459542, N'', N'', null, N'2015-12-05 12:43:40.467');
