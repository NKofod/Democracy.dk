INSERT INTO ODA.dbo.Dokument (id, typeid, kategoriid, statusid, offentlighedskode, titel, dato, modtagelsesdato, frigivelsesdato, paragraf, paragrafnummer, spørgsmålsordlyd, spørgsmålstitel, spørgsmålsid, procedurenummer, grundnotatstatus, dagsordenudgavenummer, opdateringsdato) VALUES (1003642, 17, 30, 1, N'O', N'SUU alm. del - foreløbigt svar på MFU spm. 987 om CT-skanninger, fra sundhedsministeren', N'2022-08-19 00:00:00.000', N'2022-08-19 00:00:00.000', N'2022-08-19 15:09:14.000', N'', N'', N'For 8 år siden lavede den tidligere regering en ”Jo-før-jo-bedre”-plan, der skulle give de praktiserende læger mulighed for at henvise direkte til CT-skanninger uden om kræftpakkerne for at stille hurtigere kræftdiagnoser.

For 7 år siden tillod Regionshospitalet Silkeborg de praktiserende læger at henvise til lavdosis-CT-skanning i stedet for den usikre røntgen af lungerne, hvis mistanken om lungekræft var så lille, at der ikke var indikation for at henvise til den omfattende Lungekræftpakke (det såkaldte 3. diagnostiske spor).

På baggrund af resultaterne i Silkeborg beregnede eksperter, at lavdosis CT i det 3. spor kan redde livet på flere hundrede lungekræftpatienter årligt. 
Stadiefordelingen ved nydiagnosticeret lungekræft ved lavdosis CT i det 3. diagnostiske spor svarer stort set til stadiefordelingen i verdens største screeningsstudie (NELSON-studiet).
En oversigtsartikel fra 2022 i et internationale peer review tidsskrift dokumenterer og anbefaler lavdosis CT i det 3. diagnostiske spor.

I det 3. diagnostiske spor findes ca. 2,5 pct. at have lungekræft. Forskellen på lav- og højdosis CT i et “lavincidensområde” som det 3. diagnostiske spor er så beskeden, at de ekstra kræftfund på højdosis CT opvejes af ekstra strålingsinducerede kræfttilfælde. Højdosis CT tager 3 gange så lang tid at lave og dobbelt så lang tid at beskrive i forhold til lavdosis CT.

Region Midt stoppede i 2020 efter 5 år med landets bedste resultater for nydiagnostik af lungekræft tilbuddet, som de fandt var en ”brist i patientsikkerheden”, trods massive protester fra fagfolk.
Påstanden om ”brist i patientsikkerheden” blev senere afkræftet, og den 1/7-22 har regionerne nu i fællesskab vedtaget, at alle praktiserende læger må henvise til CT-skanninger i stedet for røntgen af lungerne i det 3. diagnostiske spor.

I betragtning af den uheldige håndtering af forløbet, hvad vil sundhedsministeren da gøre for at sikre, at ordningen bliver implementeret hurtigst muligt i hele landet, så der ikke unødigt tabes flere menneskeliv?
', N'MFU spm. om CT-skanninger, til sundhedsministeren', 1003027, N'', N'', null, N'2022-08-19 15:09:26.890');
