INSERT INTO ODA.dbo.Dokument (id, typeid, kategoriid, statusid, offentlighedskode, titel, dato, modtagelsesdato, frigivelsesdato, paragraf, paragrafnummer, spørgsmålsordlyd, spørgsmålstitel, spørgsmålsid, procedurenummer, grundnotatstatus, dagsordenudgavenummer, opdateringsdato) VALUES (905652, 17, 22, 1, N'O', N'IFU alm. del - svar på spm. 52 om, hvad lovhjemlen er til, at vinduet for ansøgning for indfødsretsbevis er begrænset til alderen 21 til 22 for danske statsborgere med dobbelt statsborgerskab m.v., fra udlændinge- og integrationsministeren', N'2021-03-27 00:00:00.000', N'2021-03-27 00:00:00.000', N'2021-03-29 09:09:21.000', N'', N'', N'Hvad er lovhjemlen til, at vinduet for ansøgning for indfødsretsbevis er begrænset til alderen 21 til 22 for danske statsborgere med dobbelt statsborgerskab, jf. artiklen ”Studerende: Den danske stat gør mig til andenrangsstatsborger” fra Politiken den 19. december 2020, https://politiken.dk/debat/debatindlaeg/art8034668/Den-danske-stat-g%C3%B8r-mig-til-andenrangsstatsborger?shareToken=EqajwVAAXbmg og Ombudsmandens kritik heraf FOU nr. 2015.29, https://www.retsinformation.dk/eli/fob/2015/29?
Baggrunden for spørgsmålene, er at det ikke kun er dem, som får afslag, som er ramt af problemerne. De fleste er faktisk dem, som kan få beviset, men ikke ved, at de skal søge. Så for alle de, som er danske statsborgere, men født i udlandet og derfor kan få behov for at fremvise et bevis for dansk indfødsret, er der nogle meget vigtige spørgsmål. Mange som er født i et EU-land tror, at kravet ikke gælder dem, men at danske diplomater får en anden vejledning, når de trænes i konsulære opgaver. Derfor kan det være meget vigtigt at få afklaret, hvor mange danske statsborgere, som lever i uvidenhed om, at de en dag kan blive afkrævet deres eget eller deres forældres bevis på dansk indfødsret.
Så sagen er todelt. En ting er det omfattende bureaukrati og den lange sagsbehandlingstid i forbindelse med ansøgningen om bevis for dansk indfødsret. Bureaukratiske krav om stempling af fødselsattest eller vielsesattest i Singapore, Venezuela, Thailand mv., kan give nogle urimelige afslag. Men en meget alvorligere ting er den manglende retssikkerhed i forhold til, hvem der kan få behov for at have beviset liggende og i hvilke situationer danske statsborgere skal bruge et bevis for dansk indfødsret.
Problemet rammer danske statsborgere, herunder danskere, som udelukkende har dansk statsborgerskab. Der henvises til artiklerne ”Studerende: Den danske stat gør mig til andenrangsstatsborger” fra Politiken den 19. december 2020, https://politiken.dk/debat/debatindlaeg/art8034668/Den-danske-stat-g%C3%B8r-mig-til-andenrangsstatsborger?shareToken=EqajwVAAXbmg og ”Ikke dansk nok: Ministerium vil hverken smide familien ud – eller kalde John for dansker” fra www.dr.dk 5. februar 2020, https://www.dr.dk/nyheder/regionale/fyn/ikke-dansk-nok-ministerium-vil-hverken-smide-familien-ud-eller-kalde-john”.', N'Spm. om, hvad lovhjemlen er til, at vinduet for ansøgning for indfødsretsbevis er begrænset til alderen 21 til 22 for danske statsborgere med dobbelt statsborgerskab m.v., til udlændinge- og integrationsministeren', 894366, N'', N'', null, N'2021-03-29 09:09:33.213');
