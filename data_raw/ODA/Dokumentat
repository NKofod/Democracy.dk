INSERT INTO ODA.dbo.Dokument (id, typeid, kategoriid, statusid, offentlighedskode, titel, dato, modtagelsesdato, frigivelsesdato, paragraf, paragrafnummer, spørgsmålsordlyd, spørgsmålstitel, spørgsmålsid, procedurenummer, grundnotatstatus, dagsordenudgavenummer, opdateringsdato) VALUES (18590, 17, 22, 1, N'O', N'SAU alm. del - svar på spm. 56, fra skatteministeren', N'2012-11-15 00:00:00.000', N'2012-11-15 00:00:00.000', N'2012-11-16 08:56:28.000', N'', N'', N'Vil ministeren redegøre for, hvorvidt man ved hjælp af den neden for skitserede fremgangsmåde ville kunne komme snyd med moms mellem virksomheder til livs? Og vil ministeren, i det omfang han er enig i, at fremgangsmåden kunne være med til at forhindre snyd med moms mellem virksomheder, redegøre for ministerens planer på området, herunder om fremgangsmåden kunne indgå i disse planer?
Al handel mellem virksomheder skulle registreres via et PBS-lignende system. 
Hvis firma A sælger til firma B: 
1) Firma A opretter en elektronisk faktura via det PBS-lignende system, der indeholder navn, adresse og momsnummeret på hhv. firma A og B, betalingsdato m.v. 
2) Firma B modtager en e-mail om, at der er oprettet en faktura, hvori de er part. 
3) Firma B skal indenfor en kort tidsfrist godkende, at de har modtaget adviseringen om fakturaen. 
4) Firma B kan inden en tidsfrist vælge enten at godkende fakturaen, at afvise den totalt, eller at godkende den med ændringer.
5) Hvis firma B kan godkende fakturaen, bogføres momsbeløbet automatisk via det PBS-lignende system, hvorved ingen af firmaerne kan snyde med momsen og omsætningen. Dette kunne samtidig styre en betalingsdato via en tilknyttet konto, f.eks. Nem-konto.
6) Kan firma B ikke godkende fakturaen, skal det komme med forslag til korrektioner dertil, med den konsekvens at ingen af firmaerne kan opnå momsfradrag, før der er enighed om fakturaen. Samtidig registreres det hos SKAT, hvilke CVR nr. der er uenige om en faktura, hvorved SKAT kan føre statistik over, hvilke firmaer der har mange problemer.', N'Spm. om snyd med moms mellem virksomheder, til skatteministeren', 18589, N'', N'', null, N'2015-12-06 05:46:35.167');
