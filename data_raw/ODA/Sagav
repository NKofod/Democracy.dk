INSERT INTO ODA.dbo.Sag (id, typeid, kategoriid, statusid, titel, titelkort, offentlighedskode, nummer, nummerprefix, nummernumerisk, nummerpostfix, resume, afstemningskonklusion, periodeid, afgørelsesresultatkode, baggrundsmateriale, opdateringsdato, statsbudgetsag, begrundelse, paragrafnummer, paragraf, afgørelsesdato, afgørelse, rådsmødedato, lovnummer, lovnummerdato, retsinformationsurl, fremsatundersagid, deltundersagid) VALUES (8558, 10, 11, 20, N'Hvad vil ministeren gøre for at fjerne ulovligt fastspændingsudstyr til børn fra markedet og for at forhindre, at køretøjer kan gå gennem syn med ulovligt udstyr?', N'Om at få fjernet ulovligt fastspændingsudstyr til børn.', N'O', N'S 629', N'S', N'629', N'', N'', null, 30, null, null, N'2017-12-05 20:34:26.993', 0, N'Trafikstyrelsen udsendte den 19. juli 2011 meddelelse 1960 og en pressemeddelelse om tilbagekaldelse af ulovligt sikkerhedsudstyr til børn i busser. I meddelelse 1960 står der: 
»Synsvirksomhederne anmodes om at være opmærksom på forholdet i forbindelse med syn. Hvis udstyret ikke er godkendt, anføres på synsrapporten som servicebemærkning: »Sikkerhedsudstyr for børn ikke godkendt««.
 
Det betyder, at det ulovlige udstyr ikke medfører, at køretøjet nægtes godkendelse, og at genfremstilling til syn heller ikke er nødvendigt. 

De udsendte meddelelser oplyser ikke, hvilke virksomheder der har importeret, forhandlet og monteret de ulovlige produkter, i hvilken periode salget er foregået eller handelsbetegnelser, artikelnumre eller andre beskrivelser, som kan hjælpe med at identificere de ulovlige produkter. Det nævnes ikke, at det ulovlige udstyr også omfatter H-seler og f.eks. seleforlængere til personbiler.

Trafikstyrelsens detailforskrifter for køretøjer får ikke tilføjet de gældende regler for fastspændingsudstyr til børn, selv om reglerne ikke fremgår klart og tydeligt. Reglerne fremgår heller ikke af vejledningen om syn af køretøjer, som benyttes af synsvirksomhederne ved syn. 

Som følge heraf findes der eksempler på, at børnehave- og institutionsbusser med ulovlige seler er gået gennem syn uden påtaler og derfor ikke har fået fjernet det ulovlige udstyr.', null, null, null, null, null, null, null, null, null, null);
