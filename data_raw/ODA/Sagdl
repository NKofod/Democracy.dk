INSERT INTO ODA.dbo.Sag (id, typeid, kategoriid, statusid, titel, titelkort, offentlighedskode, nummer, nummerprefix, nummernumerisk, nummerpostfix, resume, afstemningskonklusion, periodeid, afgørelsesresultatkode, baggrundsmateriale, opdateringsdato, statsbudgetsag, begrundelse, paragrafnummer, paragraf, afgørelsesdato, afgørelse, rådsmødedato, lovnummer, lovnummerdato, retsinformationsurl, fremsatundersagid, deltundersagid) VALUES (40965, 10, 11, 20, N'Kan ministeren oplyse, hvilke regler der i dag gælder for beskatning af de honorarer, som forsøgspersoner, der indgår i medicinske forsøg, modtager?', N'Om beskatning af de honorarer, som forsøgspersoner, der indgår i medicinske forsøg, modtager.', N'O', N'S 4101', N'S', N'4101', N'', N'', null, 22, null, null, N'2017-12-07 18:12:12.423', 0, N'Dansk medicinsk forskning er internationalt kendt for at udføre forsøg på mennesker. Det er en styrke for dansk forskning og kan kun lade sig gøre, fordi vi har en tradition for, at forsøgene nøje godkendes og kontrolleres af et veludbygget etisk komitésystem og gennemføres af læger med stor klinisk erfaring. Samtidig er der i befolkningen forståelse og beredvillighed, når det gælder deltagelse i sådanne forsøg, hvor almindelige borgere bidrager til videnskaben ved at stille deres krop til rådighed. Det er derfor til gavn for dansk forskning at fastholde denne positive opbakning. Det er samtidig en fordel, at forsøg, der involverer mennesker, kan gennemføres så omkostningseffektivt og administrativt enkelt som muligt.
 
På den baggrund er det et problem, at praksis de seneste år har været at beskatte den betaling, som forsøgspersoner modtager for deres deltagelse i et forsøg. Det opfattes både urimeligt bureaukratisk af forskerne og de lønningskontorer på sygehusene, der varetager skatteadministrationen af betalingen til forsøgspersonerne. Det medfører, at forskerne må udbetale det dobbelte beløb til forsøgspersonerne, hvilket går fra forskningsmidlerne. Alt i alt er det ikke noget, der har gavnet forskningens vilkår - snarere tværtimod. Tidligere har praksis været - i hvert fald nogle steder i landet - at forsøgspersoner i forskningsprojekter ikke blev beskattet af deres honorarer.
 
Det er vigtigt at understrege, at deltagelse i forsøg er frivillig og aldrig hovedsagligt økonomisk motiveret. Betalingen skal være en godtgørelse for transporttid, transportudgifter, smerte og ubehag. Forskerne har i samarbejde med det etiske komitésystem fundet et rimeligt niveau for honoreringen. Derfor er det også klart, at på den ene side må honoreringen aldrig blive så høj, at det er en selvstændig begrundelse for at deltage. På den anden side skal den enkelte vurdere, at honoreringen står rimeligt mål med de omkostninger, der er forbundet med at deltage.', null, null, null, null, null, null, null, null, null, null);
