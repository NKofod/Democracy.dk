INSERT INTO ODA.dbo.Dokument (id, typeid, kategoriid, statusid, offentlighedskode, titel, dato, modtagelsesdato, frigivelsesdato, paragraf, paragrafnummer, spørgsmålsordlyd, spørgsmålstitel, spørgsmålsid, procedurenummer, grundnotatstatus, dagsordenudgavenummer, opdateringsdato) VALUES (245519, 17, 22, 1, N'O', N'L 107 - svar på spm. 3, fra miljøministeren.', N'2010-04-20 00:00:00.000', N'2010-04-20 00:00:00.000', N'2010-04-20 11:09:49.000', N'', N'', N'Vil ministeren fremsætte ændringsforslag til §10 i lovforslaget samt bemærkningerne hertil med følgende indhold:´

§ 10. Stk. 2. Indsatsprogrammerne skal blandt andet omfatte geografiske beskyttelsesforanstaltninger, der bidrager til et sammenhængende og repræsentativt netværk af beskyttede havområder, og som i tilstrækkelig grad dækker mangfoldigheden i de enkelte økosystemer. 

Bemærkningerne i afsnit 2.1.7 samt bemærkningerne til lovforslagets § 10:

"De geografiske beskyttelsesforanstaltninger, der fra dansk side forventes at indgå i indsatsprogrammerne, er først og fremmest de internationale naturbeskyttelsesområder samt yderligere fuldt beskyttede områder, der sammen vil udgøre et sammenhængende netværk af beskyttede områder i de danske farvande, der til fulde dækker mangfoldigheden, og som kan bidrage til opnåelse af god miljøtilstand. I forbindelse med udarbejdelsen af indsatsprogrammerne vil miljøministeren beslutte om andre typer særlige beskyttelsesområder og foranstaltninger efter anden lovgivning, f.eks. nationalparkloven eller naturbeskyttelsesloven kan indgå i foranstaltningerne”.', N'Spm. om ministeren vil fremsætte ændringsforslag til § 10 i lovforslaget, til miljøministeren', 245518, N'', N'', null, N'2015-12-05 23:37:27.293');
