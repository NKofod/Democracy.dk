INSERT INTO ODA.dbo.Sag (id, typeid, kategoriid, statusid, titel, titelkort, offentlighedskode, nummer, nummerprefix, nummernumerisk, nummerpostfix, resume, afstemningskonklusion, periodeid, afgørelsesresultatkode, baggrundsmateriale, opdateringsdato, statsbudgetsag, begrundelse, paragrafnummer, paragraf, afgørelsesdato, afgørelse, rådsmødedato, lovnummer, lovnummerdato, retsinformationsurl, fremsatundersagid, deltundersagid) VALUES (46395, 10, 11, 20, N'Vil ministeren oplyse, hvem der foretager udnævnelsen af autoriserede bobestyrere, om udnævnelsen er tidsbegrænset, og under hvilke omstændigheder en udnævnelse kan tilbagekaldes?', N'Om udnævnelsen af autoriserede bobestyrer.', N'O', N'S 1383', N'S', N'1383', N'', N'', null, 23, null, null, N'2017-12-05 23:04:17.653', 0, N'En borger har fortalt spørgeren om en række negative oplevelser, han har haft som arving i et solvent dødsbo, som blev forvaltet af en autoriseret bobestyrer.  
Disse oplevelser vil udgøre grundlaget for en række spørgsmål fra spørgeren.  For at fremme forståelsen af de kommende besvarelser anser spørgeren det for særdeles ønskeligt, at ministeren giver en kortfattet orientering om autoriserede bobestyrerer4e, deres udnævnelse og deres arbejdsopgaver m.v. 

Spørgeren kan oplyse, at den pågældende autoriserede bobestyrer er medlem af en af de største landsdækkende advokatsammenslutninger. Sammenslutningens sekretariat er gentagne gange blevet kontaktet af arvingen, men der er ikke fremkommet markante resultater herved.  Derfor er det så meget mere nødvendigt, at ministerens besvarelser kan være grundlaget for vurdering af, hvilke initiativer der vil være relevante for at sikre, at der ikke fremkommer gentagelser af tilsvarende negative boforløb. Eksempelvis kan spørgeren nævne, at alvorlige problemer med at få udarbejdet korrekte skatteopgørelser har medført, at boet endnu ikke er afsluttet, selv om der er gået 2 år, siden bobestyreren fik boet udleveret af skifteretten.  

Spørgeren kan oplyse, at spørgsmålene er baseret på boforvaltningen af et solvent dødsbo, som skifteretten i en sønderjysk købstad har overdraget til en lokal autoriseret bobestyrer, uden at arvingen blev orienteret herom.  

Arvingen har bedt spørgeren understrege, at han ikke har noget grundlag for at beklage sig over den betjening, han har fået i forbindelse med sine henvendelser til den dommerfuldmægtig, som har forestået behandlingen af de klager, som arvingen har indgivet i boforløbet.  

Endelig kan spørgeren oplyse, at såfremt ministeren måtte ønske at få kendskab til arvesagen, har arvingen bedt spørgeren meddele, at dette kan lade sig gøre uden besvær.', null, null, null, null, null, null, null, null, null, null);
